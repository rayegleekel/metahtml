        if(mobilecheck()){
            if (getReferFrom() === "baidu" || navigator.userAgent.toLowerCase().indexOf("baidu") > 0) {
                window.location.href = 'https://3g.163.com/news/article/F6V94GEH0514CKB6.html?referFrom=' + getReferFrom() + '&spss=adap_dy';
