<script>
(function () {
  function debounce(func, wait, immediate) {
    var timeout, args, context, timestamp, result;
    var later = function () {
      var last = new Date() - timestamp;

      if (last < wait && last >= 0) {
        timeout = setTimeout(later, wait - last);
      } else {
        timeout = null;
        if (!immediate) {
          result = func.apply(context, args);
          if (!timeout) context = args = null;
        }
      }
    };
    return function () {
      context = this;
      args = arguments;
      timestamp = new Date();
      var callNow = immediate && !timeout;
      if (!timeout) timeout = setTimeout(later, wait);
      if (callNow) {
        result = func.apply(context, args);
        context = args = null;
      }
      return result;
    };
  }
  var Category = (function categoryModule() {
    var defaultSelect = $('.fk__headerCell--select').text()
    var selected = defaultSelect ;
    var selectedClass = 'fk__headerCell--select';
    var list = $('#fk__headerRow1>div');
    function setSelect(element, keepOther) {
      for (var i = 0, len = list.length; i < len; i++) {
        var each = $(list[i]);
        if (!keepOther) {
          each.removeClass(selectedClass);
        }
        if (each.is(element)) {
          $(element).addClass(selectedClass);
        }
      }
    }
    function setSelectAccordCategory() {
      for (var i = 0, len = list.length; i < len; i++) {
        var each = $(list[i]);
        each.removeClass(selectedClass);
        if (each.text() === selected) {
          setSelect(each);
        }
      }
    }
    function set(category) {
      List.recordScroll(selected, Scroll.getScroll());
      selected = category;
    }
    function get() {
      return selected;
    }
    return {
      setSelect: setSelect,
      setSelectAccordCategory: setSelectAccordCategory,
      set: set,
      get: get,
      defaultSelect: defaultSelect,
    };
  })();

  var List = (function listModule() {
    
    var container = $('.fk__content ul');
    var loadingClass = 'fk__loading';
    var itemClass = 'fk__contentList';
    var PER_MAX_FETCH_TIME = 5;
  
    var ListMap = (function createMap() {
      var map = {};
      map[Category.defaultSelect] = {
        time: 1,
        html: container.html(),
        scroll: 0,
      };
      function getIfEmptySetDefault(key) {
        if (has(key)) {
          return map[key]
        }
        map[key] = {
          time: 0,
          html: '',
          scroll: Scroll ? Scroll.getWrapTop() : 0,
        };
        return map[key]
      }
      function has(key) {
        if (map[key] === undefined) {
          return false;
        }
        return true;
      }
      function set(key) {
        if (map[key] === undefined) {
          return false;
        }
        return true;
      }
      function setAllScroll(num) {
        for (var key in map) {
          if (map[key] && map[key].scroll !== undefined) {
            map[key].scroll = num
          }
        }
      }
      return {
        getIfEmptySetDefault: getIfEmptySetDefault,
        has: has,
        setAllScroll: setAllScroll,
      }
    }())
  
    var fetchThread = null;
    function imgHtml(img, url) {
      if (img) {
        return (
          '<div class="fk__contentImgWrap"><a target="_blank" href="' +
          url +
          '"><img src="' +
          img +
          '?imageView&amp;thumbnail=140y88&amp;quality=85" onerror="this.onerror=\'\';this.src=\'https://static.ws.126.net/dy/images/default180x120.jpg\'" class="fk__contentImg"/></a><span style="font-size: 0px;">*</span></div>'
        );
      }
      return '';
    }
    function infoHtml(title, url, date) {
      return (
        '<div class="fk__contentInfoWrap"><p class="fk__contentTitle"><a target="_blank" href="' +
        url +
        '">' +
        title +
        '</a></p><p class="fk__contentDate">' +
        date +
        '</p></div>'
      );
    }
    function hasFetchedMax(curCategory) {
      return ListMap.getIfEmptySetDefault(curCategory).time === PER_MAX_FETCH_TIME
    }

    function createHtml(list, curCategory) {
      var html = '';
      for (var i = 0, len = list.length; i < len; i++) {
        var each = list[i];
        var title = each.title;
        var date = each.date;
        var img = each.img;
        var url = each.url;
        html +=
          '<li class="' +
          itemClass +
          '">' +
          imgHtml(img, url) +
          infoHtml(title, url, date) +
          '</li>';
      }
      if (hasFetchedMax(curCategory)) {
        html += '<li class="fk__noMoreTip ' + itemClass + '" style="padding: 30px 0;">:-)已经到最后啦~</li>'
      }
      return html;
    }
    function scrollTo(curCategory) {
      if (Scroll && Scroll.isFixed()) {
        var scroll = ListMap.getIfEmptySetDefault(curCategory).scroll
        $(window).scrollTop(scroll)
      }
    }
    function appendScrollTo(curCategory, prevScroll) {
      if (Scroll && Scroll.isFixed()) {
        var scroll = Scroll.getScroll()
        $(window).scrollTop(scroll + scroll - prevScroll)
      }
    }
    function appendList(html) {
      container.append(html);
    }
    function replaceList(html) {
      container.html(html);
    }
    function loading() {
      container.addClass(loadingClass);
    }
    function stopLoading() {
      container.removeClass(loadingClass);
    }
    function recordScroll(category, scrollTop) {
      ListMap.getIfEmptySetDefault(category).scroll = scrollTop
    }


    function fetch(isAppend) {
      if (fetchThread) {
        fetchThread.abort();
      }
      var curCategory = Category.get();
      if (ListMap.has(curCategory)) {
        if (!isAppend) {
          replaceList(ListMap.getIfEmptySetDefault(curCategory).html);
          scrollTo(curCategory)
          return;
        }
        if (hasFetchedMax(curCategory)) {
          return;
        }
      }
      if (!isAppend) {
        loading();
      }
      var html = ''
      var prevScroll = Scroll.getScroll();
      fetchThread = $.ajax('/v2/article/category/list.do', {
        data: { cname: curCategory, pageNo:  ListMap.getIfEmptySetDefault(curCategory).time + 1},
      })
        .done(function (data) {
          try {
            data = data.data.list;
            var contentList = [];
            listSize = data.length
            for (var i = 0, len = data.length; i < len; i++) {
              var each = data[i];
              var docid = each.docid || '';
              var picUrl = each.picUrl || '';
              var title = each.title || '';
              var date = each.publishTimeStr || '';
              if (docid) {
                contentList.push({
                  img: picUrl,
                  title: title,
                  date: date,
                  url: '/article/' + docid + '.html',
                });
              }
            }
            ListMap.getIfEmptySetDefault(curCategory).time += 1;
            html = createHtml(contentList, curCategory);
          } catch (error) {
          }
        })
        .always(function () {
          fetchThread = null;
          if (!isAppend) {
            stopLoading();
          }
          ListMap.getIfEmptySetDefault(curCategory).html += html;
          if (isAppend) {
            appendList(html);
            appendScrollTo(curCategory, prevScroll);
          } else {
            replaceList(ListMap.getIfEmptySetDefault(curCategory).html);
            scrollTo(curCategory);
          }
        });
    }
    function isFetching() {
      return fetchThread !== null
    }
    return {
      fetch: fetch,
      recordScroll: recordScroll,
      setAllScroll: ListMap.setAllScroll,
      scrollTo: scrollTo,
      isFetching: isFetching
    };
  })();

  var Scroll = (function() {
    var prevScroll = 0;
    var _isFixed = false;
    var wrap = $('#fk__wrap');
    var headerTop = 50;
    var topHeaderTop = 45;
    var getWrapTop = function() {
      return wrap.offset().top;
    }
    var getFirstTop = function () {
      return getWrapTop() - headerTop - topHeaderTop;
    }
    $(window).scroll(
      function () {
        var jWin = $(window);
        var sticky = $('#fk__header');
        var scroll = jWin.scrollTop();
        var wrapOffset = getWrapTop();
        var fixedH = scroll  + headerTop +  topHeaderTop + 5
        if (!_isFixed && fixedH >= wrapOffset) {
          sticky.addClass('fk__fixed');
          _isFixed = true;
          List.setAllScroll(wrapOffset - headerTop - topHeaderTop);
        } 
        if (_isFixed && fixedH < wrapOffset) {
          sticky.removeClass('fk__fixed');
          _isFixed = false;
        }
        if (
          scroll > prevScroll &&
          (scroll >= ($(document).height() - $(window).height() - 200)) && !List.isFetching()
        ) {
          List.fetch(true);
        }
        prevScroll = scroll;
      }
    );
    var isFixed = function() {
      return _isFixed;
    }
    var getScroll = function() {
      return prevScroll
    }
    return {
      isFixed:isFixed ,
      getScroll:getScroll ,
      getWrapTop:getFirstTop,
    }
  }())
  
  $('#fk__headerRow2,#fk__headerMore').mouseleave(function (event) {
    if (
      event.toElement.id === 'fk__headerRow2' ||
      event.toElement.id === 'fk__headerMore' || 
      event.toElement.parentNode.id === 'fk__headerRow2' 
    ) {
      return;
    }
    $('#fk__headerRow2').addClass('hide');
    Category.setSelectAccordCategory();
  });
  $('#fk__headerRow2 .fk__headerCell').click(function (event) {
    var toSet = $(event.target).text();
    $('#fk__headerMore').text(toSet);
    Category.set(toSet);
    $('#fk__headerRow2').addClass('hide');
    Category.setSelectAccordCategory();
    List.fetch(false);
  });

  $('#fk__headerRow1>div').mouseenter(
    debounce(function () {
      if (this.id === 'fk__headerMore') {
        Category.setSelect(this, true);
        $('#fk__headerRow2').removeClass('hide');
      } else {
        Category.setSelect(this);
        Category.set($(this).text());
        List.fetch(false);
      }
    }, 200)
  );
})();
</script>        </div>
    </div>
