				}
		        iom.c(iam_data,1);
		        // ENDE SZM VERSION="2.0"
	        }
			// AT-INTERNET TRACKING
			trackAjaxCall(jQuery(this), 'Schlagzeilenbox - Schlagzeilen Tab', '', '', '', 'Politik', false, false, false, false, false, false);
			// ENDE AT-INTERNET TRACKING
		  }
    	});  
  	});
});
jQuery(function(){
  jQuery("#regiontab").click(function(){
    jQuery.ajax({
      url: "/?ot=de.nordbayern.modul.ot.CatchLine",
      global: false,
      type: "GET",
      data: "viewcaption=region&default=false&cid=7.24521",
      dataType: "text",
      beforeSend: function(){
        jQuery("#loading-caption").show();
        jQuery("#loading-caption").append('<i class="icon-spinner icon-spin"></i>');
      },
      success: function(data){
        jQuery("#loading-caption").hide();
        jQuery("#modul_catchline").replaceWith(data);
        if(typeof iom != "undefined") {
	        // SZM VERSION="2.0"
	        var iam_data = {
				"st":kennung,
				"cp":"schlreg",
				"sv":frabo
			}
	        iom.c(iam_data,1);
	        // ENDE SZM VERSION="2.0"
        }
		// AT-INTERNET TRACKING
		trackAjaxCall(jQuery(this), 'Schlagzeilenbox - Regionen Tab', '', '', '', 'Politik', false, false, false, false, false, false);
		// ENDE AT-INTERNET TRACKING
	  }
    });
  });
});

