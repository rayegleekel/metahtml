		<script type="text/javascript">
			var correctAnswers_74151693 = 0;
			function clickAnswer_74151693(answer) {
				var answerWasCorrect = false;
				if(answer.attr('result') == "true") {
					answer.addClass("nb-quiz-correct");
					answerWasCorrect = true;
					correctAnswers_74151693 ++;
				} else {
					answer.addClass("nb-quiz-wrong");
				}
				jQuery("#nb-quiz_74151693 .img-quest").addClass("nb-hidden");
				jQuery("#nb-quiz_74151693 .img-ans").removeClass("nb-hidden");
				jQuery("#nb-quiz_74151693 .txt-ans").removeClass("nb-hidden");
				answer.parent().children('button').each(function() {
					if(!answerWasCorrect && jQuery(this).attr('result') == "true")
						jQuery(this).addClass("nb-quiz-correct");
					jQuery(this).attr("disabled", "disabled");
					jQuery(this).css('opacity', '0.5');
					jQuery(this).css('background', jQuery(this).css('background'));
					jQuery(this).css('color', jQuery(this).css('color'));
				});
			}
			function resetQuestion_74151693() {
				jQuery("#nb-quiz_74151693 .img-quest").removeClass("nb-hidden");
				jQuery("#nb-quiz_74151693 .img-ans").addClass("nb-hidden");
				jQuery("#nb-quiz_74151693 .txt-ans").addClass("nb-hidden");
				jQuery("[id^=nb-quiz-options-74151693]:visible").children('button').each(function() {
					jQuery(this).removeAttr("disabled").removeAttr("style").removeClass("nb-quiz-correct").removeClass("nb-quiz-wrong");
				});
			}
			function nextQuestions_74151693(isResult) {
			    resetQuestion_74151693();
			    if (jQuery("#nb-quiz_74151693 .nb-quiz-element:visible").next().length != 0)
			    	jQuery("#nb-quiz_74151693 .nb-quiz-element:visible").next().show().prev().hide();
			    if(isResult) {
			    	var maxQuestion = "11";
			    	var percent = 100 / maxQuestion * correctAnswers_74151693;
			    	var resultText = "";
			    	if (percent >= 80)
						resultText = "<h3>Hervorragend! Sie haben <span class=\"nb-quiz-result\">" + correctAnswers_74151693 + "</span> von <span class=\"nb-quiz-result bold\">11</span> Fragen richtig beantwortet.</h3>";		    	
			    	else if (percent < 40)
						resultText = "<h3>Sie haben <span class=\"nb-quiz-result\">" + correctAnswers_74151693 + "</span> von <span class=\"nb-quiz-result bold\">11</span> Fragen richtig beantwortet. Das geht noch besser.</h3>";
			    	else
						resultText = "<h3>Gut gemacht. Sie haben <span class=\"nb-quiz-result\">" + correctAnswers_74151693 + "</span> von <span class=\"nb-quiz-result bold\">11</span> Fragen richtig beantwortet.</h3>";			    	
			       	jQuery("div#correctAnswers_74151693").html(resultText);
		       	}			    
		    	// SZM VERSION="2.0"
			   	jsIVWCode(isResult ? "Quiz_ergebnis" : "Quiz_naechste_Frage", kennung, frabo);
			   	// ENDE SZM VERSION="2.0"
			    // AT-INTERNET TRACKING
				trackAjaxCall(jQuery(this), isResult ? 'Quiz - Politiker in der Region - Ergebnis' : 'Quiz - Politiker in der Region - nächste Frage', '', '', '', 'Politik', false, false, false, false, false, false);
				// ENDE AT-INTERNET TRACKING
			}
			jQuery(document).ready(function() {
				jQuery("#nb-quiz-btn-intro_74151693").click(function() {
				    jQuery("#quiz-intro_74151693").hide();
			    	jQuery("#nb-quiz_74151693 .nb-hidden:first").show();
			    	// SZM VERSION="2.0"
				   	jsIVWCode("Quiz_Starten", kennung, frabo);
