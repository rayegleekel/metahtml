			    	// AT-INTERNET TRACKING
					trackAjaxCall(jQuery(this), 'Quiz - Politiker in der Region - Starten', '', '', '', 'Politik', false, false, false, false, false, false);
					// ENDE AT-INTERNET TRACKING
				});
				jQuery("#nb-quiz-btn-reintro_74151693").click(function() {
					correctAnswers_74151693 = 0;
					jQuery("#nb-quiz_74151693 .nb-quiz-element:visible").hide();
					resetQuestion_74151693();
			    	jQuery("#nb-quiz_74151693 .nb-hidden:first").show();
