		var index_74260143 = 0;
		getNextImage_74260143(index_74260143, false, false);
		function getNextImage_74260143(index, back, doTrack) {
			jQuery.ajax({
	  			url: "/?ot=de.nordbayern.modul.ot.SlideshowSnippet",
	      		global: false,
	      		type: "GET",
		      	data: "cid=74260143&galleryid=1.10134873&galleryUrl=" + encodeURI('/region/wellness-familienfeiern-und-busreisen-das-ist-seit-juni-trotz-corona-wieder-moglich-1.10134873') + "&index=" + index,
	      		dataType: "text",
		      	beforeSend: function(){
					jQuery("#loading-slideshow_74260143").show();
		      	},
	      		success: function(data) {
			        jQuery("#loading-slideshow_74260143").hide();
		         	if (data.trim() !== "") {
			        	jQuery("#modul-slideshow74260143").html(data);			        	
			        	if (doTrack) {
			        		// SZM VERSION="2.0"
						   	jsIVWCode("NB_slideshowmodul", kennung, frabo);
