            function scroll_go(){
                var ad_h=$('.bottom').offset().top;
                console.log($(window).scrollTop()+ $(window).height());
                /*--首頁側欄程式--*/
                var wh= $(window).height();


                var right_go;
                if(($('.right').height() > wh)&& ($('.right').height()< $('.box_hot .center').height())){
                    right_go=1;
                }else{
                    right_go=0;
                }


                $(window).scroll(function() {
                    if($(window).width()>=1024){
                        if(right_go===1){
                            if($(window).scrollTop()+ wh>ad_h) {
                                $('.right_box').css({position:'fixed',bottom:'0',width:'300px'});
                                $('.right').css({height:'1px'});
                            }else{
                                $('.right_box').css({position:'relative',bottom:'auto'});
                            }
                        }
                    }
                });
            }

            /*--首頁側欄程式ed--*/


            $('.content .con_box').each(function() {
                var
