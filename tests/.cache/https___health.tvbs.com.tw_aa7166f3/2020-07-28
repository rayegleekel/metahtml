        function change_index_tab(obj){
            var more = $('#tabs_more').attr('href');
                more = more.replace('/', '');
            $('#tabs_'+more).css('display', 'none');
            $('#tabs_'+obj.id).css('display', 'block');
            $('#tabs_more').attr('href', '/'+obj.id);
            $('img.lazyimage').lazyload({threshold: 500});

        }

        var last = false;
        var lock = false;
        var timer;
        $(function () {
            //總覽列表 無限瀑布流 移到最下面才會啟動
            $(window).scroll(function () {
                var document_height = $(document).height();
                var window_height = $(window).height();
                var window_scrolltop = $(window).scrollTop();
                var page = $('#page').val();
                $('img.lazyimage').lazyload({threshold: 500});
                if (window_scrolltop >= document_height - window_height - 600) {
                    if (!last && !lock) {
                        lock = true;
                        $('.combolistUlLi').show();
                        if (timer) {
                            clearTimeout(timer);
                        }
                        timer = setTimeout(function () {
                            $.get("/program/LoadMoreOverview/" + page + "/0/x324656x324654x324653x324652x324643x324633x324627x324628x324655x324650x324645x324644x324640x324636x324602x324598", function (data) {
                                if (data == '[]') {
                                    last = true;
                                    return;
                                }
                                lock = false;
                                data_array = JSON.parse(data);
                                console.log(data_array);
                                var dataTemplate = combolistTemplate(data_array);
                                // dataTemplate += combolistTemplate(data_array.slice(4, 8));
                                // dataTemplate += combolistTemplate(data_array.slice(8, 12));
                                // dataTemplate += combolistTemplate(data_array.slice(12, 16));
                                // $('#combolistUl').append(dataTemplate);
                                $('#page').val(parseInt(page) + 1);
                                $('img.lazyimage').lazyload({threshold: 500});
                            });
                        }, 300);
                    }
                }
            });
        });

        function combolistTemplate(data) {
            // var html = '';

            for (i = 0; i < data.length; i++) {
                var html = '';
                var link = data[i]['url'];
                var image = data[i]['cover_image'];
                var title = data[i]['title'];
                var publish = data[i]['publish'];

                html += "<li>";
                html += "<a href='" + link + "'>";
                html += "<div class='img'>";
                html += "<img class='lazyimage' data-original='" + image + "' src='https://cc.tvbs.com.tw/2017program/health/images/health_default.jpg' /></div>";
                // html += "<img src='" + image + "'/></div>";
                html += "<div class='txt_box'>";
                html += "<div class='title'>";
                // if(data[i]['video_id'] == null || data[i]['video_id'] == ""){
                if(data[i]['video_id'] != null && data[i]['video_id'] != ""){
                    html += "<div class='play'></div>";
                }
                html += "<span>" + title + "</span> ";
                html += "</div>";
                html += "<div class='txt'><span>"+ data[i]['article_content'] +"</span></div>";
                html += "</div>";
                html += "</a>";
                html += "<div class='tag'>";

                if(data[i]['tag'] != null && data[i]['tag'] != ''){
                    for(j = 0; (data[i]['tag']).length; j++){
                        if(j > 1){ break; }
                        html += '<p><a href="javascript:searchTag(\'14\',\''+encodeURI(data[i]['tag'][j])+'\')">'+data[i]['tag'][j]+'</a></p>';
                    }
                }else{
                    html += "<p><a style='background: rgba(0,0,0,0)' href='javascript: void(0);'></a></p>";
                }

                // html += "<p><a href='#'>營養均衡</a></p>";
                html += "</div>";
                html += "</li>";
                $('#combolistUl').append(html);
                // if((i+1) %4 == 0){
                //     //廣告
                //     // html += "<li>";
                //     // html += "<div class='ad'>";
                //     // html += "<a href='#'><img src='http://campaign.tvbs.com.tw/test/flyzero/2020health/images/admo2.jpg' alt=''/></a>";
                //     // // html += "$this->dfp_lib->show_da("v5_health_desktop_read_inarticle3", true)?>";
                //     // // html += "$this->dfp_lib->show_da("v5_health_tablet_1024_read_inarticle3", true)?>";
                //     // html += "</div>";
                //     // html += "<div class='ad_mo'>";
                //     // html += "<a href='#'><img src='http://campaign.tvbs.com.tw/test/flyzero/2020health/images/admo.jpg' alt=''/></a>";
                //     // // html += "<$this->dfp_lib->show_da("v5_health_m_read_inarticle3", true)?>";
                //     // html += "</div>";
                //     // html += "</li>";
                //     // html += $('#hot_ad').clone();
                //     // $('#combolistUl').append($('#hot_ad').clone());
                //     $('#combolistUl').append($('#combolistUl li:eq(4)').clone());
                // }
            }



            return html;
        }

