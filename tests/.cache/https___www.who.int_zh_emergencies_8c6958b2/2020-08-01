        var path_elements = new Array('__emergencies');

        function fixMenu(){
                for( i = 0; i < path_elements.length; i++ ){
                        if( document.getElementById( path_elements[i] ) ){
                                document.getElementById( path_elements[i] ).firstChild.style.color = '#D86422';
                                break;
                        }
                }
        }

