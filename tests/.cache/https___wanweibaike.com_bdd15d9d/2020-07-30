  $(window).scroll(function() {
      if ($(window).scrollTop() > 100) {
          $("#back-to-top").fadeIn(500);
      } else {
          $("#back-to-top").fadeOut(500);
      }
  });
  $("#back-to-top").click(function() {
      $('body,html').animate({
          scrollTop: 0
      },
      100);
      return false;
  });
  $('sup a').click(function(){
    var cite=$(this).attr('href');
    var cite1=cite.replace('#','');
    $('ol.references>li').css('background','#fff');
    $('#'+cite1).css('background','#ffe2e2');
  });
  
  $('table.navbox-inner tbody').each(function(i,n){
    var firstTr=$(this).children(":first");
    firstTr.addClass('default-hide');
    firstTr.siblings().addClass('hidden');
    firstTr.click(function(){
      var thisTr=$(this);
      if(thisTr.hasClass('default-hide')){
        thisTr.siblings().removeClass('hidden');
        thisTr.siblings().each(function(){
          var trSiblings=$(this);
          trSiblings.find('tr').removeClass('hidden');
        });
        thisTr.removeClass('default-hide');
      }else{
        thisTr.siblings().addClass('hidden');
        thisTr.find('tr').addClass('hidden');
        thisTr.addClass('default-hide');
      }
    }); 
      
  });
  var tableHeight=$('table.navbox').eq(0).position().top;
  if(tableHeight < 3000){
    $('table.navbox').eq(0).find('table.navbox-inner tbody').children(":first").trigger('click');
  }
  $('a.image').each(function(){
      var getSrc=$(this).find('img').attr('src');
      if(getSrc){
        $(this).attr('href',getSrc);
      }
    });
