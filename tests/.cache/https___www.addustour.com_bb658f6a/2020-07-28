<script type="74e9f02f3fcc4e5f6f84c8d1-text/Javascript">
document.getElementById("vote1").checked = true;

function writeToFile(){
	var vote = document.getElementsByName('vote');
     var vote_value;
for(var i = 0; i < vote.length; i++){
    if(vote[i].checked){
        vote_value = vote[i].value;
    }
}
			var btnVote = document.getElementById("btnvotes");
			btnVote.disabled = true	;
			btnVote.value = "شكرا على التصويت";
			var client = new HttpClient();
			
			if(vote_value === "1"){
				client.get('https://www.addustour.com/writeOnText.php?qNum=1', function(response) {});
			}else if(vote_value === "2"){
			client.get('https://www.addustour.com/writeOnText.php?qNum=2', function(response) {});
			}else if(vote_value === "3"){
			client.get('https://www.addustour.com/writeOnText.php?qNum=3', function(response) {});
			}else if(vote_value === "4"){
			client.get('https://www.addustour.com/writeOnText.php?qNum=4', function(response) {});
			}else{
			client.get('https://www.addustour.com/writeOnText.php?qNum=5', function(response) {});
			}		
}

var HttpClient = function() {
    this.get = function(aUrl, aCallback) {
        var anHttpRequest = new XMLHttpRequest();
        anHttpRequest.onreadystatechange = function() { 
            if (anHttpRequest.readyState == 4 && anHttpRequest.status == 200)
                aCallback(anHttpRequest.responseText);
        }

        anHttpRequest.open( "GET", aUrl, true );            
        anHttpRequest.send( null );
    }
}

	var client = new HttpClient();
	client.get('https://www.addustour.com/SetCookies.php?period=2592000', function(response) {});

</script>
