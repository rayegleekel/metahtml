<script type="text/javascript">
    $.getScript = function (url, cb) {
        // ä»éè·¨åå è½½åæ¢å°è·¨åå è½½ï¼éè¦å ä¸ªçæ¬å·ï¼é²æ­¢å ç¼å­å¯¼è´çé®é¢
        if (typeof url != 'string') {
            return ;
        } 
        url = url.indexOf('?') != -1 ? url + "&_cross=1" : url + "?_cross=1";
        return $.ajax({
            crossDomain: true,
            type: "GET",
            url: url,
            dataType: "script", 
            success: function (){
                typeof cb == 'function' && cb();
            },
            error: function (e) {
                typeof cb == 'function' && cb(e)
            }
        });
    }
