<h2 class="conHeadline">Zahlen & Fakten</h2>
<div class="modCon">
<div class="mod modD modTeaseronly" data-ctrl-slidable="{'id':'zahlenpager','action':{'default':['disableWhenNotNeeded']}}">
<div class="boxCon slider" aria-live="polite" data-ctrl-layoutable="{'id':'infografikindex100-bilder','action':{'default':['equalHeight']},'timeToWait':'350','refreshOnAdded':'true'}" data-ctrl-zahlenpager-slider="{}">
