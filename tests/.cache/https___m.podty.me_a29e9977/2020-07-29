            // ~ Episode detail
            var $episodeDetail = $('#episodeDetail');
            $episodeDetail.find('[name="playBtn"]').click(function() {
                var eventCategory = '에피소드 상세 - ';

                var $button = $(this);
                var format = $episodeDetail.data('format');
                var type;
                if (format == 'mp3')
                    type = 'AOD';
                else if (format == 'vod')
                    type = 'VOD';
                else if (format == 'pdf')
                    type = 'PDF';

                eventCategory += type;

                var eventAction;
