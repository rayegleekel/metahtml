                gaSendEvent('방송 상세', '에피소드 명', episodeName);
            });
            $episodeList.find('button').click(function() {
                var $button = $(this);
                var format = $button.data('format');

                var eventAction;
                if ($button.hasClass('btn_pause') || $button.hasClass('btn_pdf'))
                    eventAction = '재생';
                else
                    eventAction = '일시정지';

                var eventLabel;
                if (format == 'mp3')
                    eventLabel = 'AOD';
                else if (format == 'vod')
                    eventLabel = 'VOD';
                else if (format == 'pdf')
                    eventLabel = 'PDF';

                eventLabel += ' - ' + $button.closest('li').find('.tit_txt').text();

                gaSendEvent('방송 상세', eventAction, eventLabel);
            });
		}, 300);
	});
</script>    </div>

    <footer class="footer">
        <a href="javascript:changeViewMobileToPc();" class="btn_link">PC 버전으로 보기</a>
