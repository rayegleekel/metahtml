    <script>
        var mousedowning = false;
        var sliding = false;

        function initBanners() {
            var $rollingBanners = $('#slider').lightSlider({
                item: 1,
                auto: true,
                loop: true,
                pause: 3000,
                controls: false,
                slideMargin: 0,
                pauseOnHover : true,
                speed: 400,
                onSliderLoad: function() {
                    $rollingBanners.removeClass('cS-hidden');

                    var $links = $rollingBanners.find('a');
                    $links.off('click').on('click', onClickBanner);

                    $links.mousedown(function() {
                        mousedowning = true;
                    });

                    $links.mouseup(function() {
                        if (!sliding)
                            $links.off('click').on('click', onClickBanner);

                        mousedowning = false;
                    });

                    $links.mousemove(function() {
                        if (mousedowning) {
                            $links.off('click').on('click', offClickBannerToDoNothing);
                            sliding = true;
                        } else
                            sliding = false;
                    });

                    $('.lSPager').hide();
                },
                onAfterSlide: function() {
                    $rollingBanners.find('a').off('click').on('click', onClickBanner);
                    sliding = false;
                }
            });
        }

        function offClickBannerToDoNothing(e) {
            e.preventDefault();
        }

        function onClickBanner(e) {
            e.preventDefault();

            var $link = $(e.target).closest('a');
            podty.pjax.go($link.attr('href'), $link);

            gaSendEvent('홈-배너', '배너', $link.data('title'));
        }

        function popupCategories() {
            $('body').css('visibility', 'hidden');
            $('.ly_category').show().css('visibility', 'visible');
            bodyScrollLock.disableBodyScroll($('#categoriesPopup .pop_contents')[0]);
        }

        $(document).ready(function() {
            podtyPlayer.setPlayButtons(['.episodeList .bx_dummy', '.episodeList .bx_align', '.area_banner button'], 'index');

            initBanners();
            goToApp();
        });
    </script>

        <div id="categoriesPopup" class="ly_pop_wrap ly_category" style="display:none">
            <div class="pop_contents">
                <div class="group_item">
