                </div>
                <button type="button" class="btn_close" onclick="closeCategory();"><span class="sp">닫기</span></button>
            </div>
        </div>
<script>
	window.ga=window.ga||function(){(ga.q=ga.q||[]).push(arguments)};ga.l=+new Date;

	gaSendEvent = function(eventCategory, eventAction, eventLabel) {

		eventCategory = eventCategory || "";
		eventAction = eventAction || "";
		eventLabel = eventLabel || "";

		if(eventCategory == "" || eventAction == ""){
			return;
		}

		if(eventLabel == "") {
			ga('send', 'event', {
				eventCategory : eventCategory,
				eventAction : eventAction
			});
		}else{
			ga('send', 'event', {
				eventCategory : eventCategory,
				eventAction : eventAction,
				eventLabel : eventLabel
			});
		}
	};

	$(document).ready(function() {
		setTimeout(function() {

			ga('create', 'UA-125890633-1', 'auto');
			// ~ 페이지뷰
			var page = location.pathname;

			if (page.length > 1 && page.substring(0, 2) == '/m')
			    page = page.substr(2);
			if (!page)
			    page = '/';

            var pageTitle;
			if (page == '/' || page == '/index')
				pageTitle = '메인 - 페이지';
			else if (page == '/intro')
				pageTitle = '메인 - 팟티 소개';
			else if (page == '/choice/list')
				pageTitle = '팟티초이스 상세';
			else if (page == '/chart/podty/daily')
				pageTitle = '팟티 차트 상세';
			else if (page == '/chart/daily')
				pageTitle = '카테고리 차트 상세';
			else if (page == '/new/episodes')
				pageTitle = '새로운 에피소드 상세';
			else if (page == '/new/episodes')
				pageTitle = '새로운 에피소드 상세';
			else if (page == '/search/total')
				pageTitle = '검색 결과 - 통합';
			else if (page == '/search/cast')
				pageTitle = '검색 결과 - 방송';
			else if (page == '/search/episode')
				pageTitle = '검색 결과 - 에피소드';
            else if (/\/episode\/\d+$/.test(page))
                pageTitle = '에피소드 페이지';
            else if (/\/cast\/\d+$/.test(page))
                pageTitle = '방송 상세';
            else if (page == '/popup/kbs/2019')
                pageTitle = '이벤트 페이지 - 팝업 - KBS 2019';
            else if (page == '/popup/kbs/2019/detail')
                pageTitle = '이벤트 페이지 - 상세 - KBS 2019 - 모바일 웹';

            if (pageTitle)
				ga('send', 'pageview', pageTitle);
			else
                ga('send', 'pageview');

            var $container = $('body > .wrap');
            var $header = $container.find('> .header');
            $header.find('.logo').off('click').on('click', function() {
                gaSendEvent('헤더', 'BI 버튼');
            });
            $header.find('.top_area .btn_search').off('click').on('click', function() {
                gaSendEvent('헤더', '검색 버튼');
            });

            var $appBanner = $container.find('> .bx_appbanner2');
            $appBanner.find('.btn_link').off('click').on('click', function() {
