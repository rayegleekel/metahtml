    slide_bildstrecke +='</div>';
  
    $('#teaser_bildstrecke .imageholder').append(slide_bildstrecke);
  }
}

$('#teaser_bildstrecke #bildstrecke_forward_bt').click(function() {
current_pos_bildstrecke = current_pos_bildstrecke-element_width_bildstrecke;
current_page_bildstrecke++;
if ( current_page_bildstrecke == loaded_page_bildstrecke ) {
  loaded_page_bildstrecke++;
  add_page_bildstrecke(loaded_page_bildstrecke);
}
update_bildstrecke();
});

$('#teaser_bildstrecke #bildstrecke_back_bt').click(function() {
current_pos_bildstrecke = current_pos_bildstrecke+element_width_bildstrecke;
update_bildstrecke();
current_page_bildstrecke--;
});

