var current_pos_bildstrecke = 0;
var element_width_bildstrecke = 462;

var current_page_bildstrecke = 0;
var loaded_page_bildstrecke = 1;

function update_bildstrecke () {
  $("#teaser_bildstrecke .imageholder").animate( { left:current_pos_bildstrecke+"px" });
  if ( current_pos_bildstrecke == 0 ) {
    $("#teaser_bildstrecke .back").css("display", "none");
  }
  else {
    $("#teaser_bildstrecke .back").css("display", "block");
  }

  var width_imagewrap_bildstrecke = $("#teaser_bildstrecke .doublewrap").size()*(element_width_bildstrecke/2);
  if ( current_pos_bildstrecke <= element_width_bildstrecke-width_imagewrap_bildstrecke ) {
    $("#teaser_bildstrecke .forward").css("display", "none");
  }
  else {
    $("#teaser_bildstrecke .forward").css("display", "block");
  }
}

function add_page_bildstrecke ( pagenum ) {
  var id_bildstrecke_start = (pagenum-2)*4;
  
  if (diashow_htmlteaser_json[id_bildstrecke_start]!=null) {
    quarter_bildstrecke = new Array();  
    for (var counter=0;counter<=1;counter++) {
      quarter_bildstrecke[counter] = '';
      quarter_bildstrecke[counter]+='<div class="quarterwrap">';
      quarter_bildstrecke[counter]+='<table><tr><td data-vr-contentbox="Link '+diashow_htmlteaser_json[id_bildstrecke_start+(counter*2)].url+'"><a href="'+diashow_htmlteaser_json[id_bildstrecke_start+(counter*2)].url+'">'+diashow_htmlteaser_json[id_bildstrecke_start+(counter*2)].name+'</a></td></tr></table>';
      quarter_bildstrecke[counter]+='<a href="'+diashow_htmlteaser_json[id_bildstrecke_start+(counter*2)].url+'"><img src="'+diashow_htmlteaser_json[id_bildstrecke_start+(counter*2)].src+'" /></a>';
      quarter_bildstrecke[counter]+='</div>'; 
      quarter_bildstrecke[counter]+='<div class="quarterwrap">';
      quarter_bildstrecke[counter]+='<a href="'+diashow_htmlteaser_json[id_bildstrecke_start+1+(counter*2)].url+'"><img src="'+diashow_htmlteaser_json[id_bildstrecke_start+1+(counter*2)].src+'" /></a>';
