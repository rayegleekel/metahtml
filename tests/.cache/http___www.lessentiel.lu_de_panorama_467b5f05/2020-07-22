<div class="box full x42" data-vr-zone="Story Right 23" data-vr-contentbox="" data-ana-click='{"event": "placement_click", "cfg":{"dependent":"placement_view"} ,"data": {"view": 2013,"type"        : "html","elementid"   : 5,"revision"    : 0,"row"         : 58,"col"         : 4,"style"       : 0,"width"       : 4,"height"      : 2,"auto"        : ""}}'  data-ana-inview='{"event": "placement_view" ,"data": {"view": 2013,"type"        : "html","elementid"   : 5,"revision"    : 0,"row"         : 58,"col"         : 4,"style"       : 0,"width"       : 4,"height"      : 2,"auto"        : ""}}'>
<script language="JavaScript">
  function istickertabready() {
    if($('#ticker_news_scrollwrapper')[0])
        {
      $('#ticker_news_tab').click();
        }
    else
      setTimeout("istickertabready()", 100);
  } 
</script>



<script type="text/javascript">
  //function istickertabready() {
    //if($('#ticker_news_scrollwrapper')[0])
    //  $('#ticker_news_tab').click();
    //else
    //  setTimeout("istickertabready()", 100);
  //}

  $(document).ready(function(){
    setTimeout("istickertabready()", 100);
    // tabfades
    $("#ticker_news_tab").click(function () {
      fadeouttickers();
      settickertabs( "ticker_news_tab" );
      $("#ticker_news").fadeIn("slow");
    });
    $("#ticker_sport_tab").click(function () {
      fadeouttickers();
      settickertabs( "ticker_sport_tab" );
      $("#ticker_sport").fadeIn("slow");
    });
    $("#ticker_unterhaltung_tab").click(function () {
      fadeouttickers();
      settickertabs( "ticker_unterhaltung_tab" );
      $("#ticker_unterhaltung").fadeIn("slow");
    });

    function fadeouttickers () {
      $("#ticker_news").fadeOut("fast",position_content);
      $("#ticker_sport").fadeOut("fast",position_content);
      $("#ticker_unterhaltung").fadeOut("fast",position_content);

    }

    function settickertabs ( current_tab ) {
      $("#ticker_news_tab").removeClass("active");
      $("#ticker_sport_tab").removeClass("active");
      $("#ticker_unterhaltung_tab").removeClass("active");

      $("#"+current_tab).addClass("active");
    }

    function position_content () {
      $("#ticker_news").css("top","60px");
      $("#ticker_sport").css("top","60px");
      $("#ticker_unterhaltung").css("top","60px");

    }

  });
</script>
