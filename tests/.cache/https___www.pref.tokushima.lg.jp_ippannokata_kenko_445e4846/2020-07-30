<script>
	//組織から探すボタン設定
	if (document.querySelector('.dev-pc .temp_side_soshiki .text_item')) {
		elm = document.querySelector('.dev-pc .temp_side_soshiki .text_item');
		elm.innerHTML = '組織から探す';
