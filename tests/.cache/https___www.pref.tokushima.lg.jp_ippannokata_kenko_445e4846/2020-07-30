        $(function () {
          var original_device = $.cookie('original_device'); //2:tb, 3:sp
          if (original_device == 'tb') {
            $('.to_original_page a').text('徳島県ホームページ（タブレット版）表示');
          } else if (original_device == 'sp') {
            $('.to_original_page a').text('徳島県ホームページ（スマートフォン版）表示');
          } else {
            $('.to_original_page a').text('徳島県ホームページ表示');
            $('.to_original_page').css('display', 'none');
          }
          $(document).on('click', '.to_original_page a', function () {
            var original_device = $.cookie('original_device');
            var device_type;
            if (original_device == 'tb') {
              device_type = 2;
            } else if (original_device == 'sp') {
              device_type = 3;
            }
            $.cookie('selected_device', device_type, {expires: 1, path: '/', domain: 'www.pref.tokushima.lg.jp'});window.location.href = $(location).attr('pathname');
          });
        });
  </script><script>
  var x = 100;
  var cnt = 0;

  function larger() {
    if (cnt >= 2) {
      return;
    }
    x *= 1.2;
    cnt++;
    document.body.style.fontSize = x + '%';
  }
  function medium() {
    x = 100;
    cnt = 0;
    document.body.style.fontSize = 100 + '%';
  }

  function smaller() {
    x /= 1.2;
    cnt--;
    document.body.style.fontSize = x + '%';
  }
</script></body></html>
