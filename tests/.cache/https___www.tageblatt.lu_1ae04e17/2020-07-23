<script>

function showHide()
{


  var x = document.getElementById('navmenu');
  var y = document.getElementById('headernavbarLeft');



  if(x.style.display != 'block')
  {
    x.style.display = 'block';
    y.innerHTML = 'X'
  }else
  {
    x.style.display = 'none';
    y.innerHTML = '<img style="width : 20px; height : 20px;" src="https://www.tageblatt.lu/wp-content/themes/theme-tageblatt-v01/Ressources/images/Pictos/Picto-menu.svg"/>';
  }


}


</script>

<div class="row">
    <div id="headernavbar_div" class="col-12 ">
        <div class="container " >
            <div class="row">
              <div id="headernavbar" onclick="showHide()" class="col-12 m-0 p-0">
