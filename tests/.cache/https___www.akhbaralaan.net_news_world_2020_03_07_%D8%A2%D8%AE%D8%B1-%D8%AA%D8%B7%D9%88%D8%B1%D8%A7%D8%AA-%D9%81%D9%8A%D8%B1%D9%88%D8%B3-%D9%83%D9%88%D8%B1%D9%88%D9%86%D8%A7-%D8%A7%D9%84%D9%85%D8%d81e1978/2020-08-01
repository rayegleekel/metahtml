        function move_widget_for_device() {
            var window_width = jQuery(window).width();
            if(jQuery('.main-story-content').length){
                var ms_width = jQuery('.main-story-content').width();
                var ms_height= (ms_width*9)/16;
                jQuery('.main-story-content img').css('max-height', ms_height);
            }
            if (window_width <= 767) {
                /*move some widget after the sticky post widget*/
                if(jQuery('.move-on-top-wdgt').length){
                    if(!jQuery('.moved-on-top-wdgt').length){
                        jQuery('.home-main-content-widgets .wpb_widgetised_column .wpb_wrapper div:first').after("<div class='moved-on-top-wdgt'></div>");
                    }
                    jQuery('.move-on-top-wdgt').each(function(){
                    if(!jQuery(this).hasClass('hidden')){
                        jQuery(this).addClass('hidden');
                        jQuery('.moved-on-top-wdgt').append('<div class="widget">'+jQuery(this).html()+'</div>');
                    }
                    })
                }
                /* if(jQuery('.move-on-top-wdgt').length && !jQuery('.move-on-top-wdgt').hasClass('hidden') ){
                    let mtw = jQuery('.move-on-top-wdgt').html();
                    jQuery('.home-main-content-widgets .wpb_widgetised_column .wpb_wrapper div:first').after("<div class='widget moved-on-top-wdgt'>"+mtw+"</div>");
                    jQuery('.move-on-top-wdgt').addClass('hidden');
                } */

                /*Move Right Sidebar to center*/
                let rw = jQuery('.home-right-sidebar-widget .wpb_widgetised_column .wpb_wrapper').html();
                jQuery('.move-right-sidebar').html(rw);
                jQuery('.home-right-sidebar-widget').hide();

                /*Move Left Sidebar to center*/
                let lw = jQuery('.home-left-sidebar-widget .wpb_widgetised_column .wpb_wrapper').html();
                jQuery('.move-left-sidebar').html(lw);
                jQuery('.home-left-sidebar-widget').hide();

                /*move first child*/
                let lwfc = jQuery('.home-left-sidebar-widget .wpb_widgetised_column .wpb_wrapper .widget:first-child').html();
                jQuery('#lwfc').remove();
                jQuery('.move-right-sidebar').prepend("<div class='widget' id='lwfc'>"+lwfc+"</div>");
                jQuery('.move-left-sidebar .widget:first-child').hide();
    

            } else if (window_width <= 991) {

                /*move first child*/
                jQuery('.home-left-sidebar-widget .wpb_widgetised_column .wpb_wrapper .widget:first-child').show();
                jQuery('#lwfc').remove();

                /*show left sidebar*/
                jQuery('.move-left-sidebar').html('');
                jQuery('.home-left-sidebar-widget').show();
    
                /*move some widget after the sticky post widget*/
                if(jQuery('.move-on-top-wdgt').length){
                    if(!jQuery('.moved-on-top-wdgt').length){
                        jQuery('.home-main-content-widgets .wpb_widgetised_column .wpb_wrapper div:first').after("<div class='moved-on-top-wdgt'></div>");
                    }
                    jQuery('.move-on-top-wdgt').each(function(){
                    if(!jQuery(this).hasClass('hidden')){
                        jQuery(this).addClass('hidden');
                        jQuery('.moved-on-top-wdgt').append('<div class="widget">'+jQuery(this).html()+'</div>');
                    }
                    })
                }

                /*Move Right Sidebar to center*/
                let rw = jQuery('.home-right-sidebar-widget .wpb_widgetised_column .wpb_wrapper').html();
                jQuery('.move-right-sidebar').html(rw);
                jQuery('.home-right-sidebar-widget').hide();

            } else if (window_width >= 992) {
                jQuery('.move-right-sidebar').html('');
                jQuery('.home-right-sidebar-widget').show();
                jQuery('.move-left-sidebar').html('');
                jQuery('.home-left-sidebar-widget').show();
                jQuery('.move-on-top-wdgt').removeClass('hidden');
                jQuery('.moved-on-top-wdgt').remove();
            }
        }
