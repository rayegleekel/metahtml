jQuery(document).on('click','span.copyurl',function(e){
    var url = jQuery(this).attr('data-href');
    jQuery(this).attr('title','Link copied');
    jQuery(this).html('<i class="fa fa-check" style="color:green;" aria-hidden="true"></i>');
    var temp = jQuery("<input>");
        jQuery("body").append(temp);
        temp.val(url).select();
        document.execCommand("copy");
        temp.remove();        
    });
