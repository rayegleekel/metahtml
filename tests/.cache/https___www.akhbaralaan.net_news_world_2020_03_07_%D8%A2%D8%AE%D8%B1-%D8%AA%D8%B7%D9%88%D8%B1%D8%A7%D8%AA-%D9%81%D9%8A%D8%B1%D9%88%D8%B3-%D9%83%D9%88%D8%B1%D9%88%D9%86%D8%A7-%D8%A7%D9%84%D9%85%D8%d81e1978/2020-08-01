    jQuery(document).ready(() => {
        setTimeout(function() {
            if (!sessionStorage.getItem('hide_fb_signup')) {
                if (!Cookies.get('alaan_user_id')) {
                    jQuery.ajax({
                        url: 'https://www.akhbaralaan.net/wp-content/themes/alaan/facebook-sdk/login.php', cache: false, dataType: 'json', async: false,
                        success: function (result) {
                            jQuery('#fb_signup_btn').attr('href', result.fb_login_url);
                            jQuery('#signup-with-fb').animate({height: 250}, 500).css({'padding':'15px 15px 50px'});
                        }
                    });
                    jQuery('.close-fb').click(function () {
                        jQuery('#signup-with-fb').hide();
                        sessionStorage.setItem('hide_fb_signup', 1);
                    });
                } else {
                    jQuery('#signup-with-fb').hide();
                }
            } else {
                jQuery('#signup-with-fb').hide();
            }
        }, 45 * 1000);
    });
