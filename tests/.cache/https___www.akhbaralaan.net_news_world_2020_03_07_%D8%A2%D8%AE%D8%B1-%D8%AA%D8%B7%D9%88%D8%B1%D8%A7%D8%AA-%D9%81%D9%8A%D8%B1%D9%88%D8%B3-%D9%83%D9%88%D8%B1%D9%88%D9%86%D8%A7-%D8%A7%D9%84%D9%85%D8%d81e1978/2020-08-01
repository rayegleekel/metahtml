    function addNonNullPhint(key, value) {
            if ((typeof value !== 'undefined') && (value !== null) && (value != '') && (value != 'undefined')) {
                bk_addPageCtx(key, value);
            }
        }
        window.bk_async = function () {
            bk_allow_multiple_calls = true;
            bk_use_multiple_iframes = true;
            var ref = document.referrer;
            if (ref) {
                if ((ref.includes('google.') || ref.includes('yahoo.') || ref.includes('bing.') || ref.includes('facebook.') || ref.includes('linkedin.'))) {
                    if (window.location.href.includes('utm')) {
                        bk_addPageCtx('paid', 'true');
                    } else {
                        bk_addPageCtx('organic', 'true');
                    }
                }
            }
            addNonNullPhint('url_pathname', window.location.pathname);
            addNonNullPhint('url_hostname', window.location.hostname.replace("www.", ""));
            var url_params = window.location.search;
            addNonNullPhint('url_params', window.location.search);
            if (url_params) {
                var up_array = url_params.substring(1).split("&");
                for (var i = 0; i < up_array.length; i++) {
                    var pair = up_array[i].split('=');
                    if (pair[0] && pair[1]) {
                        addNonNullPhint("up_" + pair[0].toLowerCase(), pair[1].toLowerCase());
                    }
                }
            }
            metas = document.getElementsByTagName('meta');
            for (let i = 0; i < metas.length; i++) {
                if (metas[i].getAttribute('property') === "article:tag") {
                    addNonNullPhint('article_tag', metas[i].getAttribute('content'));
                }
            }
            BKTAG.doTag(82683, 4);
        };
        (function () {
            var scripts = document.getElementsByTagName('script')[0];
            var s = document.createElement('script');
            s.async = true;
            s.src = "//tags.bkrtx.com/js/bk-coretag.js";
            scripts.parentNode.insertBefore(s, scripts);
        }());
    </script>
