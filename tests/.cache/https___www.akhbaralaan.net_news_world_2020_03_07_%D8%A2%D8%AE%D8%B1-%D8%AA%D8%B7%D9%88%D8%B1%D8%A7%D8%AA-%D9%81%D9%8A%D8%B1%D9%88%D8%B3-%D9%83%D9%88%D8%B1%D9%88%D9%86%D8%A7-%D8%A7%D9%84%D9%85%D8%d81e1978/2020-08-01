                <script>
       function which_browser() {
            var browser = '';
            if (navigator.userAgent.indexOf("Chrome") !== -1) {
                browser = 'Chrome';
            } else if (navigator.userAgent.indexOf("Firefox") !== -1) {
                browser = 'Firefox';
            } else if (navigator.userAgent.indexOf("Safari") !== -1 && navigator.userAgent.indexOf("Chrome") === -1 && navigator.userAgent.indexOf("Firefox") === -1) {
                browser = 'Safari';
            }
            return browser;
        } 

        function load_webp_images() {
            var imgDefer = document.getElementsByTagName('img');
            for (var i = 0; i < imgDefer.length; i++) {
                var img_data_src = imgDefer[i].getAttribute('data-src');
                var img_src = imgDefer[i].getAttribute('src');
                if(which_browser()!='Safari' && img_data_src !== null){

                    if(img_data_src.search("webp")===-1){
                        img_data_src = img_data_src.replace('loc.alaan.tv','wpcdn.alaan.tv/filters:format(webp)');
                        img_data_src = img_data_src.replace('dj2zdk7ii5yuk.cloudfront.net','dj2zdk7ii5yuk.cloudfront.net/filters:format(webp)');
                        img_data_src = img_data_src.replace('alaanstgcdn.alaan.tv','alaanstgcdn.alaan.tv/filters:format(webp)');
                        img_data_src = img_data_src.replace('wpcdn.alaan.tv','wpcdn.alaan.tv/filters:format(webp)');
                        img_data_src = img_data_src.replace('wpstgcdn.alaan.tv','wpstgcdn.alaan.tv/filters:format(webp)');
                    }
                    imgDefer[i].setAttribute('data-src', img_data_src);
                }
                
            }
        }

        load_webp_images();

    </script></head>
