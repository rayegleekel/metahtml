var is_login = getCookie('alaan_user_id');
var window_width = $( window ).width();
    if(typeof is_login != 'undefined' && is_login != ''){
        if(window_width > 1024){
            var link = '<a href="/view-profile?key='+is_login+'"><img height="27" width="27" class="circle" src="https://graph.facebook.com/'+is_login+'/picture?type=square"></a>';
            $('.user-avatar-link').html(link).show();
            $('.user-avatar-link').parent('.jeg_nav_item').css({'padding-left':'0px'});
            $('.akhbaralaan-usr-link').parent('.jeg_nav_item').css({'padding':'0px'});
            $('.alaanfm-usr-link').parent('.jeg_nav_item').css({'padding':'0px'});
            if($('.user-avatar-link').hasClass('akhbaralaan-usr-link')){
                $('.user-avatar-link').siblings().css({'display':'inline-block'});
                $('.user-avatar-link').css({'display':'inline-block','padding-right':'14px'});
            }
        }else{
            $.ajax({
                type: 'POST',
                dataType: 'json',
                url: 'https://www.akhbaralaan.net/wp-admin/admin-ajax.php',
                data: {action: 'get_user_profile_data',fb_id: is_login},
                success: function (result) {
                    if(result.code == 'success'){
                        $('.jeg_mobile_menu').append('<li class="menu-item menu-item-type-custom menu-item-profile-link menu-item-object-custom bgnav"><a  href="/view-profile?key='+is_login+'"><img height="27" width="27" class="circle" src="https://graph.facebook.com/'+is_login+'/picture?type=square"> '+result.username+'</a></li>');
                    }
                }
            });
        }
    } else{
        $('.user-avatar-link').parent('.jeg_nav_item').hide();
    }

$('.view-profile-form').submit(function(e){
            e.preventDefault();
        var user_name = $('input[name="user_name"]').val();
        var fb_id = $('input[name="fb_id"]').val();
        var form_data = $(this).serialize();
        var notify = 1;
        if($('input[name="notification"]'). prop("checked") == true){
            notify = 1;
        }else{
            notify = 0;
        }
        $.ajax({
                type: 'POST',
                dataType: 'json',
                url: 'https://www.akhbaralaan.net/wp-admin/admin-ajax.php',
                data: {action: 'update_user_profile_data', username: user_name,fb_id: fb_id,notify: notify,form_data :form_data },
                success: function (result) {
                    $('.msg-panel').addClass('panel-'+result.code).show().html(result.msg).fadeOut(4000);
                }
            });
            
        });        
        if(jQuery('.corona-live-count-widget').length){        
            jQuery.ajax({
                url: 'https://corona.lmao.ninja/v2/all',
                dataType: 'json',
                success: function(data) {
                    if(data){
                        jQuery('.clc_total_count').html(data.cases.toLocaleString('en-US'));
                        jQuery('.clc_death_count').html(data.deaths.toLocaleString('en-US'));
                        jQuery('.clc_recovered_count').html(data.recovered.toLocaleString('en-US'));
                    }
                }
            });
        }        
        if(jQuery('.prayer-time-container').length){
            if (navigator.geolocation) {
                navigator.geolocation.getCurrentPosition(function(position){
                    jQuery.getJSON('https://api.aladhan.com/v1/timings?latitude='+position.coords.latitude+'&longitude='+position.coords.longitude+'&method=4', function (times)
                    {
                        if(times.code == 200){
                            show_prayer_times(times,'marked');
                            setInterval(function() { show_prayer_times(times, 'marked'); }, 5000);
                            jQuery('.prayer-time-container').removeClass('hidden');                        
                        }
                    });
                }, function(e){ get_default_times(); });
            }else{                
                get_default_times();
            }
        /**
	 * when abbreviations are mouseover-ed show a tooltip with the data from the title attribute
	 */	
	jQuery(document).on('mouseover','.ask-permission', function() {		
            var ths = jQuery(this);
            // first remove all existing abbreviation tooltips
            ths.next('.al-tooltip').remove();
            ths.after('<span class="al-tooltip">' + ths.attr('data-title') + '</span>');
            var left = 0;
            var top = ths.position().top + ths.height()+ 15;
            ths.next().css('left',left);
            ths.next().css('top',top);
	});
	
	/**
	 * when abbreviations are clicked trigger their mouseover event then fade the tooltip
	 * (this is friendly to touch interfaces)
	 */
	jQuery(document).on('click','.ask-permission', function(){		
            jQuery(this).mouseover();		
            // after a slight 2 second fade, fade out the tooltip for 1 second
            jQuery(this).next().animate({opacity: 0.9},{duration: 2000, complete: function(){
                    jQuery(this).fadeOut(1000);
            }});		
	});
	
	/**
	 * Remove the tooltip on abbreviation mouseout
	 */
	jQuery(document).on('mouseout','.ask-permission', function(){			
            jQuery(this).next('.al-tooltip').remove();
	});
        
        }
    });
    function allow_location_btn(){
        jQuery('.prayer-time-container .jeg_block_heading').append('<a href="javascript:void(0);" data-title="يرجى تمكين الموقع" style="padding-top: 3%;" class="ask-permission pull-left "><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="white" width="18px" height="18px"><path d="M0 0h24v24H0z" fill="none"/><path d="M20.94 11c-.46-4.17-3.77-7.48-7.94-7.94V1h-2v2.06c-1.13.12-2.19.46-3.16.97l1.5 1.5C10.16 5.19 11.06 5 12 5c3.87 0 7 3.13 7 7 0 .94-.19 1.84-.52 2.65l1.5 1.5c.5-.96.84-2.02.97-3.15H23v-2h-2.06zM3 4.27l2.04 2.04C3.97 7.62 3.25 9.23 3.06 11H1v2h2.06c.46 4.17 3.77 7.48 7.94 7.94V23h2v-2.06c1.77-.2 3.38-.91 4.69-1.98L19.73 21 21 19.73 4.27 3 3 4.27zm13.27 13.27C15.09 18.45 13.61 19 12 19c-3.87 0-7-3.13-7-7 0-1.61.55-3.09 1.46-4.27l9.81 9.81z"/></svg></a>');
   }
    function get_default_times(){
        allow_location_btn();
        jQuery.getJSON('https://api.aladhan.com/v1/timings?latitude='+25.276987+'&longitude='+55.296249+'&method=4', function (times)
        {
            if(times.code == 200){
                show_prayer_times(times, 'default');
                setInterval(function() { show_prayer_times(times,'default'); }, 5000);
                jQuery('.prayer-time-container').removeClass('hidden');
                jQuery('<span>(مكة المكرمة)</span>').insertAfter('.time-counter-hours');
//                jQuery('.time-counter').append('');
            }
        });             
    }
    function show_prayer_times(times, type){
        var fajr = times.data.timings.Fajr.split(':');
        var sunset = times.data.timings.Sunset.split(':');
        var currTime = new Date;
        if(type == 'default'){
            utc = currTime.getTime() + (currTime.getTimezoneOffset() * 60000);
            currTime = new Date(utc + (3600000*3));
        }
        var fajrTime = new Date( currTime.getFullYear(),currTime.getMonth(),currTime.getDate(), fajr[0],fajr[1] );
        var sunsetTime = new Date( currTime.getFullYear(),currTime.getMonth(),currTime.getDate(), sunset[0],sunset[1] );
        var curr_time = currTime.getTime();
        var fajr_time = fajr[0]+':'+fajr[1];
        var sunset_time = sunset[0]-12;
        sunset_time = '0'+sunset_time+':'+sunset[1];
        jQuery('.prayer-times .pull-right').html(' فجر'+' '+fajr_time);
        jQuery('.prayer-times .pull-left').html(' مغرب'+' '+sunset_time);
        if(curr_time > fajrTime.getTime() && curr_time <= sunsetTime.getTime()){
            show_timecounter(2,sunsetTime,currTime, type );
        }
        if( curr_time <= fajrTime.getTime()){
            show_timecounter(1,fajrTime,currTime, type );
        }
        if(curr_time > sunsetTime.getTime()){
            show_timecounter(3,fajrTime,currTime, type );
        } 
    }
    function show_timecounter(type,time, curr_time, timezone){
        if(type == 1){
            jQuery('.prayer-time-container .prayer-time-label').html('باقي على الفجر');
            jQuery('.prayer-time-container').addClass('fajr-time');
            jQuery('.prayer-time-container').removeClass('sunset-time');
        }
        if(type == 2){
            jQuery('.prayer-time-container .prayer-time-label').html('باقي على الإفطار');
            jQuery('.prayer-time-container').addClass('sunset-time');
            jQuery('.prayer-time-container').removeClass('fajr-time');
        }
        if(type == 3){
            jQuery('.prayer-time-container .prayer-time-label').html('باقي على الفجر');
            jQuery('.prayer-time-container').addClass('fajr-time');
            jQuery('.prayer-time-container').removeClass('sunset-time');
            time.setDate(time.getDate() + 1);
        }     
        var now = new Date();
        if(timezone == 'default'){
            utc = now.getTime() + (now.getTimezoneOffset() * 60000);
            now = new Date(utc + (3600000*3));
        }
        var endTime = (Date.parse(time) / 1000);
        var currTime = (Date.parse(now) / 1000);
        var timeLeft = endTime - currTime;

        var days = Math.floor(timeLeft / 86400); 
        var hours = Math.floor((timeLeft - (days * 86400)) / 3600);
        var minutes = Math.floor((timeLeft - (days * 86400) - (hours * 3600 )) / 60);

        if (hours < "10") { hours = "0" + hours; }
        if (minutes < "10") { minutes = "0" + minutes; }

        jQuery(".time-counter-hours").html(hours);
        jQuery(".time-counter-minutes").html(minutes);
    }    
    
function getCookie(cname) {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for(var i = 0; i <ca.length; i++) {
          var c = ca[i];
          while (c.charAt(0) == ' ') {
            c = c.substring(1);
          }
          if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
          }
        }
        return "";
    }
