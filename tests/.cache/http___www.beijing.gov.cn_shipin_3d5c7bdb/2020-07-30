<!--引入jq架包-->
<script src="
/shipin/Portal/Tpl/templates//newspbj/js/jquery-1.11.1.min.js"></script>
<script src="
/shipin/Portal/Tpl/templates//newspbj/js/jquery.terseBanner.min.js"></script>
<script src="
/shipin/Portal/Tpl/templates//newspbj/js/idangerous.swiper2.7.6.js"></script>
<script>
    $(".idxCount").html($(".banner").find("li").length);
    /** banner图效果 **/
    $(".banner").terseBanner({
        auto: 2000,
        init: function($banner, $item) {

        },
        after: function ($banner, $item, currentIndex) {
            var idx = currentIndex + 1;
            if (idx > $(".banner").find("li").length) {
                idx = 1;
            }
            /** 当前的显示第几个 **/
            $(".idx1").html(idx);
            /** 自定义取出列表信息的名称 **/
            $(".banText").html($($banner).find("a").eq(idx).attr("title"));
            $(".banText").attr("title", $($banner).find("a").eq(idx).attr("title"));
        }
    });
    /** 访谈搜索 **/
    $(".ft_search").click(function () {
        var keyWord = $(this).parent("form").find("input").val();
        window.open("http://www.beijing.gov.cn/shipin/index.php?option=newspbj,search&searchWord="+keyWord);
    })
    $(".ft_right").find("input").on("focus", function () {
        if ($(this).val() == '请输入关键字...') {
            $(this).val("");
        }
    })
    $(".ft_right").find("input").blur(function () {
        if ($(this).val() == '') {
            $(this).val("请输入关键字...");
        }
    })
    var recordSrc = ''; // 存储src路径
    /** 访谈才单划入和划出 **/
    $(".ft_right").find("li").mouseover(function () {
        var index = $(this).find("img").attr("data-index");
        recordSrc = $(this).find("img").attr("src");
        $(this).find("img").attr("src", "/shipin/Portal/Tpl/templates/newspbj/images/fangtan/active_ft_icon" + index + ".png");
    })
    $(".ft_right").find("li").mouseout(function () {
        $(this).find("img").attr("src", recordSrc);
    })
    /** 专栏部分切换功能 **/
    var mySwiper = new Swiper('.swiper-container', {
        slidesPerView: 3,
        spaceBetween: 45,
        slidesPerGroup: 1,
        prevButton: '.swiper-button-prev',
        nextButton: '.swiper-button-next',
    })
    $('.swiper-button-prev').on('click', function (e) {
        e.preventDefault()
        mySwiper.swipePrev()
    })
    $('.swiper-button-next').on('click', function (e) {
        e.preventDefault()
        mySwiper.swipeNext()
    })
</script>