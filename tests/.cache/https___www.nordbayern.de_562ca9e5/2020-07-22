		<script type="text/javascript">
			var correctAnswers_74312428 = 0;
			function clickAnswer_74312428(answer) {
				var answerWasCorrect = false;
				if(answer.attr('result') == "true") {
					answer.addClass("nb-quiz-correct");
					answerWasCorrect = true;
					correctAnswers_74312428 ++;
				} else {
					answer.addClass("nb-quiz-wrong");
				}
				jQuery("#nb-quiz_74312428 .img-quest").addClass("nb-hidden");
				jQuery("#nb-quiz_74312428 .img-ans").removeClass("nb-hidden");
				jQuery("#nb-quiz_74312428 .txt-ans").removeClass("nb-hidden");
				answer.parent().children('button').each(function() {
					if(!answerWasCorrect && jQuery(this).attr('result') == "true")
						jQuery(this).addClass("nb-quiz-correct");
					jQuery(this).attr("disabled", "disabled");
					jQuery(this).css('opacity', '0.5');
					jQuery(this).css('background', jQuery(this).css('background'));
					jQuery(this).css('color', jQuery(this).css('color'));
				});
			}
			function resetQuestion_74312428() {
				jQuery("#nb-quiz_74312428 .img-quest").removeClass("nb-hidden");
				jQuery("#nb-quiz_74312428 .img-ans").addClass("nb-hidden");
				jQuery("#nb-quiz_74312428 .txt-ans").addClass("nb-hidden");
				jQuery("[id^=nb-quiz-options-74312428]:visible").children('button').each(function() {
					jQuery(this).removeAttr("disabled").removeAttr("style").removeClass("nb-quiz-correct").removeClass("nb-quiz-wrong");
				});
			}
			function nextQuestions_74312428(isResult) {
			    resetQuestion_74312428();
			    if (jQuery("#nb-quiz_74312428 .nb-quiz-element:visible").next().length != 0)
			    	jQuery("#nb-quiz_74312428 .nb-quiz-element:visible").next().show().prev().hide();
			    if(isResult) {
			    	var maxQuestion = "11";
			    	var percent = 100 / maxQuestion * correctAnswers_74312428;
			    	var resultText = "";
			    	if (percent >= 80)
						resultText = "<h3>Hervorragend! Sie haben <span class=\"nb-quiz-result\">" + correctAnswers_74312428 + "</span> von <span class=\"nb-quiz-result bold\">11</span> Fragen richtig beantwortet.</h3>";		    	
			    	else if (percent < 40)
						resultText = "<h3>Sie haben <span class=\"nb-quiz-result\">" + correctAnswers_74312428 + "</span> von <span class=\"nb-quiz-result bold\">11</span> Fragen richtig beantwortet. Das geht noch besser.</h3>";
			    	else
						resultText = "<h3>Gut gemacht. Sie haben <span class=\"nb-quiz-result\">" + correctAnswers_74312428 + "</span> von <span class=\"nb-quiz-result bold\">11</span> Fragen richtig beantwortet.</h3>";			    	
			       	jQuery("div#correctAnswers_74312428").html(resultText);
		       	}			    
		    	// SZM VERSION="2.0"
			   	jsIVWCode(isResult ? "Quiz_ergebnis" : "Quiz_naechste_Frage", kennung, frabo);
			   	// ENDE SZM VERSION="2.0"
			    // AT-INTERNET TRACKING
				trackAjaxCall(jQuery(this), isResult ? 'Quiz - Literaturquiz - Ergebnis' : 'Quiz - Literaturquiz - nächste Frage', '', '', '', '', false, false, false, false, false, false);
				// ENDE AT-INTERNET TRACKING
			}
			jQuery(document).ready(function() {
				jQuery("#nb-quiz-btn-intro_74312428").click(function() {
				    jQuery("#quiz-intro_74312428").hide();
			    	jQuery("#nb-quiz_74312428 .nb-hidden:first").show();
			    	// SZM VERSION="2.0"
				   	jsIVWCode("Quiz_Starten", kennung, frabo);
				   	// ENDE SZM VERSION="2.0"
			    	// AT-INTERNET TRACKING
					trackAjaxCall(jQuery(this), 'Quiz - Literaturquiz - Starten', '', '', '', '', false, false, false, false, false, false);
					// ENDE AT-INTERNET TRACKING
				});
				jQuery("#nb-quiz-btn-reintro_74312428").click(function() {
					correctAnswers_74312428 = 0;
					jQuery("#nb-quiz_74312428 .nb-quiz-element:visible").hide();
					resetQuestion_74312428();
			    	jQuery("#nb-quiz_74312428 .nb-hidden:first").show();
			    	// SZM VERSION="2.0"
				   	jsIVWCode("Quiz_Erneut_Starten", kennung, frabo);
				   	// ENDE SZM VERSION="2.0"
			    	// AT-INTERNET TRACKING
					trackAjaxCall(jQuery(this), 'Quiz - Literaturquiz - Erneut Starten', '', '', '', '', false, false, false, false, false, false);
					// ENDE AT-INTERNET TRACKING
				});
				jQuery("#nb-quiz_74312428 .nb-quiz-answer").click(function() {
				    clickAnswer_74312428(jQuery(this));
				    // SZM VERSION="2.0"
				   	jsIVWCode("Quiz_Abgestimmt", kennung, frabo);
				   	// ENDE SZM VERSION="2.0"
				    // AT-INTERNET TRACKING
					trackAjaxCall(jQuery(this), 'Quiz - Literaturquiz - Abgestimmt', '', '', '', '', false, false, false, false, false, false);
					// ENDE AT-INTERNET TRACKING
				});
			});
		</script>
