            function spanChangeCaptionboxRegion(idSelect1, idSelect2){
              var indexIdSelect1 = document.getElementById(idSelect1).selectedIndex;
              var selected_text = document.getElementById(idSelect1).options[indexIdSelect1].text;
              document.getElementById(idSelect2).innerHTML = selected_text;
              selectvalue = document.getElementById(idSelect1).value;
              captionRegionFilterKlick(selectvalue, 'region');
            }
