			<script type="text/javascript" src="https://ue.nordbayern.de/cre-1.0/tracking/tracking.js"></script>
		<script type="text/javascript">
			/* start:nbcropmark_celeraone_listener_function */			
			var creListener = function(response) {
				try {
	                if (typeof(dataLayer) !== 'undefined')
	                    dataLayer.push({'event': 'c1TrackingResponse', 'c1TrackingResponse': response});
	     			var data = response.data;
	     			handleCmsSnippet(data, "/");
				} catch (e)  {
				    // should prevent a destroyed page: should never happen
			  	}
            };
            /* end:nbcropmark_celeraone_listener_function */
			if(typeof cre_client != "undefined") {
			  	/* start:nbcropmark_celeraone_vars */
			  						cre_client.set_channel("Home");
													cre_client.set_cms_id("2.244");
													cre_client.set_content_id("/");
													cre_client.set_doc_type("ressortPage");
													cre_client.set_entitlement("free");
																					cre_client.set_origin("web");
													cre_client.set_service_id("nordbayern");
													cre_client.set_site("nordbayern.de");
																	cre_client.set_tag("Nachrichten, News, Nordbayern, Franken, Mittelfranken, Oberpfalz, Nuernberg, Fuerth, Erlangen, Kino, Restaurants, Sport, Fussball, Polizeibericht, Blaulicht");
								cre_client.set_page_view();
				/* end:nbcropmark_celeraone_vars */
				/* start:nbcropmark_celeraone_listener */
				cre_client.addListener(creListener);
				/* end:nbcropmark_celeraone_listener */
				console.log("cre_client", cre_client);
				cre_client.request();		
			}
		</script>
