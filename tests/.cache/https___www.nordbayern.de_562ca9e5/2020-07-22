					trackAjaxCall(jQuery(this), 'Loginbox - Einloggen Button', '', '', '', '', false, false, false, false, false, false);
					// ENDE AT-INTERNET TRACKING
				}
     		}
  		});
	}
	/**
	 * Removes duplicates in the array 'a'
	 */
	function unique(a) {
	  tmp = new Array(0);
	  for(i=0;i<a.length;i++){
	    if(!contains(tmp, a[i])){
	      tmp.length+=1;
	      tmp[tmp.length-1]=a[i];
	    }
	  }
	  return tmp;
	}
	/**
	 * Returns true if 's' is contained in the array 'a'
	 */
	function contains(a, e) {
		for(j=0;j<a.length;j++)if(a[j]==e)return true;
	  	return false;
	}
	jQuery(document).ready(function() {
		jQuery("#nbLoginForm_logindiv").find('input[name="login_email"]').keydown(function(event) {
