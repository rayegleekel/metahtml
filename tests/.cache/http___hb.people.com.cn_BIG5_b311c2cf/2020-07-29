<script type="text/javascript">
RMWSSO.getUserData(function(user){
	if(!user.userId){
		$("#txz_dlq").show();
		$("#txz_dlh").hide();
		//alert('æªç»é');
	}else{
		$("#txz_dlq").hide();
		$("#txz_dlh").show();
		var loginMsg = user.nickname + "ï¼æ­¡è¿æ¨"  ; 
		$("#loginMsg").html(loginMsg);
	}
});

function loginSuccess(){
	$("#txz_dlq").hide();
	$("#txz_dlh").show();
	RMWSSO.getUserData(function(user){
		if(user.userId){
		var loginMsg = user.nickname + "ï¼æ­¡è¿æ¨"  ; 
		$("#loginMsg").html(loginMsg);
		}
	})
	$("#p_login").css({"display":"none"})
}

$("#logout").bind('click', function(event){
	RMWSSO.logout(function(){
		//alert('éåºæåï¼')
		$("#txz_dlq").show();
		$("#txz_dlh").hide();
	});
})
