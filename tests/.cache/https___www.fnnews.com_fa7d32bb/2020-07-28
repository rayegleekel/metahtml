			main_add_html +="<div id='div-gpt-ad-1496653987464-0' class='ad_bodyno2' style='height:600px; width:160px;'>";
			main_add_html +="<script>googletag.cmd.push(function() { googletag.display('div-gpt-ad-1496653987464-0'); });<\/script>";
