	     	btn+= '<a href="#" class="ico_hugs stop" id="pauseButton"></a>';
	     	btn+= '<a href="#" class="ico_hugs play" id="playButton"></a>';
	     	btn+= '</span></div>';
	    $('.owl-carousel').append(btn)
	}
