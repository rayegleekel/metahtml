<script language="javascript" type="text/javascript"> 
var oMarquee = document.getElementById("g1");  //æ»å¨å¯¹è±¡
var iLineHeight = 90;  //åè¡é«åº¦ï¼åç´ 
var iLineCount =7;  //å®éè¡æ°
var iScrollAmount = 1;  //æ¯æ¬¡æ»å¨é«åº¦ï¼åç´ 
function run() {
oMarquee.scrollTop += iScrollAmount;
if ( oMarquee.scrollTop == iLineCount * iLineHeight )
oMarquee.scrollTop = 0;
