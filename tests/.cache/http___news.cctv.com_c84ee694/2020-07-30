	function getData(id){
		var now = new Date();
		var year = now.getFullYear();
		var month = (now.getMonth() + 1) < 10 ? '0' + (now.getMonth() + 1) : now.getMonth() + 1;
		var date = now.getDate() < 10 ? '0' + now.getDate() : now.getDate();
		weaId = id;	
		expdate = new Date();
		var h = expdate.getHours();
		if (h<10){h = "0" +h;}
		loadScript("//ip.apps.cntv.cn/json/weather.do",function () {
			id = getAddress()[0];
			var city_arr = weatherObject.forecast['24h'][id]['1001001'];
			var bx= city_arr[0]['001'];
			var bx1= city_arr[1]['001'];
			var gw= parseInt(city_arr[0]['003']);
			var dw = parseInt(city_arr[0]['004']);
			var gw1= parseInt(city_arr[1]['003']);
			var dw1 = parseInt(city_arr[1]['004']);
			var tdate=h;
			var Release=parseInt(tdate);
			var gao ="" ;
			var di = ""; 
			if(Release>=18||Release<=8){Meteorological=bx1;dw=dw;gw=gw1;}else{Meteorological=bx;gw=gw;dw=dw;}		
			if(!gw){gw=gw1}
			if(!bx){Meteorological=bx1}
			var weather=Meteorological;
			if (gw>dw){gao=gw;di=dw;}else{gao=dw;di=gw;}
			weaGao = gao;
			weaDi = di;
			weartherText = new StringBuffer();
			weartherText.append('<div class="data"><span class="date">').append(year+"å¹´"+month+"æ"+date+"æ¥").append('</span><span class="location"><i></i>').append(getAddress()[1]).append('</span><span class="weather">').append(weather).append(' ').append(weaGao).append('â').append('/').append(weaDi).append('â').append('</span><span class="weather_search"><a href = "http://weather.cntv.cn/" target = "_blank">å¤©æ°æ¥è¯¢></a></span></div>');
			document.getElementById('weather').innerHTML = weartherText.toString();			
		},"");
	}
	
	
	function loadScript(__stringUrl, __functionCallback, __stringCharset) {
	var __elementScript = document.createElement("script");
	__elementScript.type = "text/javascript";
	if (typeof __stringCharset != "undefined") {
	__elementScript.charset = __stringCharset;
	}
	__elementScript._functionCallback = typeof __functionCallback != "undefined" ?  __functionCallback : new Function();
	__elementScript[document.all ? "onreadystatechange" : "onload"] = function () {
	if (document.all && this.readyState != "loaded" && this.readyState != "complete") {return;}
	this._functionCallback(this);
	this._functionCallback = null;
	this[document.all ? "onreadystatechange" : "onload"] = null;
	this.parentNode.removeChild(this);
	};
	__elementScript.src = __stringUrl;
	document.getElementsByTagName("head")[0].appendChild(__elementScript);
	}
	var cookieValue = getCookie('_CCTV_CURRENT_CITY');
	if(cookieValue==""){
	getData("0");
	loadScript("//ip.apps.cntv.cn/json/weather.do",function () {
	id = getAddress()[0];
	getData(id);
	var curdate = new Date();
	curdate.setTime(curdate.getTime() + (1000*60*60*24));
	SetCookie('_CCTV_CURRENT_CITY',id,curdate);
	},"");
	}else{
