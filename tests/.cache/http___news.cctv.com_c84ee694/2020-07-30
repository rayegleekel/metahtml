	function lunboload(){
		var ind01_silde=$(".xinwen18886_ind01 .list_con .silde");
		var ind01_silde_lenght=$(".xinwen18886_ind01 .list_con .silde").length;
		var ind01_left=$(".xinwen18886_ind01 .leftbtn");
		var ind01_right=$(".xinwen18886_ind01 .rightbtn");
		var ind01index=0;
		var isFlag=true;
		var setmove;
		var ind01index_cur="";
		if(ind01_silde_lenght<2){
			$(".xinwen18886_ind01 .page").hide();
			$(".xinwen18886_ind01 .btn").hide();
			
		}else{
			$(".xinwen18886_ind01 .page .all").html(ind01_silde_lenght);
			$(".xinwen18886_ind01 .page").show();
			$(".xinwen18886_ind01 .btn").show();
			
		}
		
		ind01_right.click(function(){
			ind01index++;
				
			if(ind01index>=ind01_silde_lenght){
				ind01index=0;
			}
	
			ind01move(ind01index);
		})
		ind01_left.click(function(){
			ind01index--;
			if(ind01index<0){
				ind01index=ind01_silde_lenght-1;
			}
			
			ind01move(ind01index);
		})
	
