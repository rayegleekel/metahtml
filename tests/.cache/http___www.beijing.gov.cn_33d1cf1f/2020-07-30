<script type="text/javascript">
// 公务员信箱非本域名跳转
var nouserpass=false
function mailLogin() {
    var user = document.getElementById('u-uid');
    var pass= document.getElementById('u-password');
    if (user.value == '' || pass.value == '') {
        alert(document.domain);
        return true;
    }else{
        return false;
    };
};
//要闻区选项卡
$('.tab_hd h2').hover(function(){
  var index = $('.tab_hd h2').index(this);
if(index==0){
 $(".qlwMore").show(); 
}else if(index!=0){
 $(".qlwMore").hide();
$(".qlwBox").hide();
}
  $(this).addClass('cur').siblings().removeClass('cur');
  $('.tab_bd ul').eq(index).show().siblings('ul').hide();
})
//我要找政策选项卡
$('.policy_hd h3').on("click",function(){
  var index = $('.policy_hd h3').index(this);
  $(this).addClass('cur').siblings().removeClass('cur');
  $('.policy_bd>div').eq(index).show().siblings('div').hide();
  $('.policy_box span').text("全部");
  $('.policy_box ul').hide();
})
//优化营商环境选项卡
$(".tab_hd2 h2").hover(function(){
    var index = $('.tab_hd2 h2').index(this);
      $(this).addClass('cur').siblings().removeClass('cur');
      $(this).addClass('cur').stop().animate({
        width:"660px"
      },1000).siblings().removeClass('cur').stop().animate({
        width:"540px"
      },1000)
      $('.tab_bd2>div').eq(index).show().siblings('div').hide();
});
//图片切换
$('.slider2').flexslider({
    animation: "slide",
    animationLoop: true,
    directionNav:true,
    controlNav:true,
    itemWidth: 250,
    itemMargin: 40,
    maxItems: 3
});
//添加动效
$('.suggestions_cont li').hover(function(){
    $(this).find('span').addClass('bounce animated');
},function(){
	$(this).find('span').removeClass('bounce animated');
});
//国务院信息
            $(function(){
                $.ajax({
                    url :'http://www.gov.cn/pushinfo/v150203/pushinfo.jsonp',
                    dataType : "jsonp",
                    scriptCharset:'UTF-8', 
                    jsonp : "pushInfoJsonpCallBack",
                    jsonpCallback:"pushInfoJsonpCallBack",
                    success : function(data) {
                        $.each(data,function(i,json){
                            if (i >= 7){return false;}
                            $(".council_list").append("<li><a target='_blank' title='"+json.title+"' href='"+json.link+"'>"+json.title+"</a></li>")
                        });
                    },
                     error:function(){
                        //alert("错误");
                    }
                });
            })

//换一换
var _flip1 = 0;
$("#btn_refresh").on("click",function(e){
    if (_flip1++ % 2 === 0){
        $(".tab_hot_news2").show().siblings("ul").hide();
    }else{
        $(".tab_hot_news1").show().siblings("ul").hide();
    }
    e.preventDefault();
});
//搜索热词
$(function(){
        var tempWord = $.trim($('#hotWord').text());
        var wordArray = tempWord.split(/[,，]/);
        var hotOne = wordArray[0];
        var normalTxt = '大家在搜  ';
        $('#keyword').val(normalTxt + hotOne);
        var html = "<div class='hot-start fl'>热搜词：</div>";
        html+= "<div class='clearfix hot-word fl'>";
        var count = 0;
        // 首都之窗搜索框加长 词汇限制长度为10 其他界面长度限制为 5
        for(var i=0; i<wordArray.length  && i < 10   ; i++){
            if(wordArray[i] != ""){
                html += "<a href='http://www.beijing.gov.cn/so/s?tab=all&siteCode=1100000088&qt="+encodeURIComponent(wordArray[i])+"' target='_blank' class='fl'>" + wordArray[i] + "</a>";
                count++;
            }
        }
        html += "</div>";
        html += "</div>";
        if(count > 0){
            $("#hotWord").html(html);
            $("#hotWord").show();
        }
        
        //站内搜索
        function znForm(){
          var temp = searchForm.temp.value;
          var url = "http://www.beijing.gov.cn/so/s?tab=all&siteCode=1100000088&qt="+encodeURIComponent(temp);
          if(temp == null || temp=="" || temp == "全文搜索" || temp == normalTxt + hotOne){
            window.open("http://www.beijing.gov.cn/so");
          }
          else{
            window.open(url);
          }
          return false;
        }
$("#keyword").focus(function(){
	var txt_value = $(this).val();
	if(txt_value==normalTxt + hotOne){
		$(this).val("");
	}
});
$("#keyword").blur(function(){
	var txt_value = $(this).val();
	if(txt_value==""){
		$(this).val(normalTxt + hotOne);
	}
});
//默认搜索引擎   --   搜索暂时只显示政策，其他隐藏
 $("#search").on("submit",znForm);
    });
</script>
