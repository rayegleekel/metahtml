<script>
//预警信息弹框
$(function(){
    $(".index_btm_tips .tips_closed").on('click',function(){
        $(this).parents(".index_btm_tips").fadeOut();
    });
    $(".index_btm_tips2 .tips_closed").on('click',function(){
        $(this).parents(".index_btm_tips2").fadeOut();
    });
    if($(".index_btm_tips .hidden_area_tit").text() =="on"){
        $(".index_btm_tips").show();
                    $(".index_btm_tips2").hide();
    }
     if($(".index_btm_tips2 .hidden_area_tit").text() =="on"){
        $(".index_btm_tips2").show();
                    $(".index_btm_tips").hide();
    }
});
</script>
