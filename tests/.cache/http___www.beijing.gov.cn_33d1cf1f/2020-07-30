      <script type="text/javascript">
      //我要找政策下拉框
      $(".category_search_item").on("click",function(event){
        event.stopPropagation();
        $(this).children("ul").show();
        $(this).find("span").addClass("cur");
        $(this).addClass("z_mark");
      })
      $(".category_search_item li").on("click",function(){
        var i = $(".category_search_item li").index(this);
        _txt3 = $(".category_search_item li").eq(i).text();
        $(".category_search_item span").text(_txt3);
        $(".category_search_item span").removeClass("cur");
        $(".category_search_item ul").hide();
        return false;
      });
      //关闭下拉框
      $(".g_index").on("click",function(event){
        if($(".category_search_item").hasClass("z_mark")){
            $(".category_search_item ul").hide();
            $(".category_search_item").removeClass("z_mark");
            $(".category_search_item span").removeClass("cur");
        }
      })
      $(".g_index").on("click",function(event){
        if($(".policy_box").hasClass("z_mark")){
            $(".policy_box ul").hide();
            $(".policy_box").removeClass("z_mark");
            $(".policy_box span").removeClass("cur");
        }
      })
      //我要找政策
      $(".policy_box").each(function(){
        $(this).on("click",function(event){
          event.stopPropagation();
          $(this).find("ul").show().parent().siblings().find("ul").hide();
          $(this).find("span").addClass("cur");
          $(this).addClass("z_mark");
        })
      });

      $(".policy_box1 li").on("click",function(){
        var i = $(".policy_box1 li").index(this);
        _txt1 = $(".policy_box1 li").eq(i).text();
        $(".policy_box1 span").text(_txt1);
        $(".policy_box1").find("input").val(_txt1);
        $(".policy_box1 span").removeClass("cur");
        $(".policy_box1 ul").hide();
        return false;
      });
      $(".policy_box2 li").on("click",function(){
        var i = $(".policy_box2 li").index(this);
        _txt2 = $(".policy_box2 li").eq(i).text();
        $(".policy_box2 span").text(_txt2);
        $(".policy_box2").find("input").val(_txt2);
        $(".policy_box2 span").removeClass("cur");
        $(".policy_box2 ul").hide();
        return false;
      });
      //我要找政策跳转
      function checkForm(){ 
            var str = Math.round(Math.random()*10000);
            // var type = $("#zcST").val();
            var adx = $("#adx").val();
            var asmzq = $("#asmzq").val();
            var url='http://www.beijing.gov.cn/zhengce/zcdh?token=' + str +'&adx='+encodeURIComponent(adx)+'&asmzq='+encodeURIComponent(asmzq)+'&type='+1
            window.open(url);
        }
        function checkForm2(){
            var str = Math.round(Math.random()*10000);
            // var type = $("#zcST").val();
            var adx = $("#adx2").val();
            var asmzq = $("#asmzq2").val();
            var url='http://www.beijing.gov.cn/zhengce/zcdh?token=' + str +'&adx='+encodeURIComponent(adx)+'&asmzq='+encodeURIComponent(asmzq)+'&type='+0
            window.open(url);
        }
        //文号、标题搜索
        $(function () {
        	function search(){
        		var value = $('#action-input').val();

	            if ($("#select").val() == 0){
	                var temp = value.split(" ");
	                $("#docwz").val(temp[0]);
	                $("#docnf").val(temp[1]);
	                $("#docJtwh").val(temp[2]);
	            }else {
	                $("#qt").val(value);
	                $("#docwz").val("");
	                $("#docnf").val("");
	                $("#docJtwh").val("");
	            }

	            $('#actionForm').submit()
        	}
        $('#action-btn').click(function () {
            search();
        });
        $("#action-input").keydown(function (e) {
            if (e.keyCode == 13) {
                search();
            }
        });
        $('#action-input').click(function () {
            if ( "京发改规 2019 1" ===this.value){
                this.value = ""
            }
        });
    });
      </script>
      <!--我要找政策 e-->
