				loginklick_logindivmobile();
				return false;
			 }
		});
		jQuery("#nbLoginForm_logindivmobile").find('input[name="login_password"]').keydown(function(event) {
			if (event.keyCode == 13) {
				loginklick_logindivmobile();
				return false;
			 }
		});
	});
