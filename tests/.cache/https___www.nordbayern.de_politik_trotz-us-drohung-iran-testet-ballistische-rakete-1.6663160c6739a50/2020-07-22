<script type="text/javascript">
	<!--
	function loginklick_logindivmobile(){
  		var serializedData = jQuery("#nbLoginForm_logindivmobile").find('input[name="login_forward"]').serialize() + "&" + jQuery("#nbLoginForm_logindivmobile").find('input[name="login_referer"]').serialize() + "&" + jQuery("#nbLoginForm_logindivmobile").find('input[name="login_div"]').serialize() + "&" + jQuery("#nbLoginForm_logindivmobile").find('input[name="login_email"]').serialize() + "&" + jQuery("#nbLoginForm_logindivmobile").find('input[name="login_password"]').serialize(); 
  		serializedData = serializedData.replace(/[^&]+=\.?(?:&|$)/g, '');
  		var params = new Array();
  		params = serializedData.split('&');
  		var paramstmp = unique(params);
		var params = paramstmp[0] + "&" + paramstmp[1];  
  		if (paramstmp[2] != "" && typeof(paramstmp[2])!="undefined")
  			params = params + "&" + paramstmp[2];
  		if (paramstmp[3] != "" && typeof(paramstmp[3])!="undefined") {
  			var paramsEmail = new Array();
  			paramsEmail = paramstmp[3].split('=');
  			params = params + "&" + paramsEmail[0] + "=" + encode64(paramsEmail[1]);
		}
  		if (paramstmp[4] != "" && typeof(paramstmp[4])!="undefined") {
  			var paramsPassword = new Array();
  			paramsPassword = paramstmp[4].split('=');
  			params = params + "&" + paramsPassword[0] + "=" + encode64(paramsPassword[1]);
  		}
  		jQuery.ajax({
      		url: "/loginservlet",
      		global: false,
      		type: "POST",
      		data: params,
      		dataType: "text",
      		beforeSend: function(){
        		jQuery("div#loading-loginbox_logindivmobile").show();
        		jQuery("div#loading-loginbox_logindivmobile").html('<i class=\"icon-spinner icon-spin\"></i>');
      		},
      		success: function(data, textStatus, xhr){	  
		  		jQuery("div#loading-loginbox_logindivmobile").hide();
        		if (xhr.getResponseHeader("login_success") == "true"){
          			window.location.replace(xhr.getResponseHeader("login_successurl"));
        		} else if (xhr.getResponseHeader("password_expired_url")) {
					window.location.href = xhr.getResponseHeader("password_expired_url");
				} else {
        			var dataArray = new Array();
        			dataArray = data.split('id="logindivmobile">');       
        			if(dataArray[1] != "" && typeof(dataArray[1])!="undefined"){
          				var data2Array = new Array();
          				data2Array = dataArray[1].split('<!--');
          				if(data2Array[0] != "" && typeof(data2Array[0])!="undefined"){
            				var logindivData = jQuery.trim(data2Array[0]);
          				}
        			}
					jQuery("div#logindivmobile").html(logindivData);
					if(typeof iom != "undefined") {
	        			// SZM VERSION="2.0"
	        			var iam_data = {
							"st":kennung,
							"cp":"login",
							"sv":frabo
						}
	        			iom.c(iam_data,1);
	        			// ENDE SZM VERSION="2.0"
        			}
					// AT-INTERNET TRACKING
					trackAjaxCall(jQuery(this), 'Loginbox - Einloggen Button', '', '', '', 'Politik', false, false, false, false, false, false);
					// ENDE AT-INTERNET TRACKING
				}
     		}
  		});
	}
	/**
	 * Removes duplicates in the array 'a'
	 */
	function unique(a) {
	  tmp = new Array(0);
	  for(i=0;i<a.length;i++){
	    if(!contains(tmp, a[i])){
	      tmp.length+=1;
	      tmp[tmp.length-1]=a[i];
	    }
	  }
	  return tmp;
	}
	/**
	 * Returns true if 's' is contained in the array 'a'
	 */
	function contains(a, e) {
		for(j=0;j<a.length;j++)if(a[j]==e)return true;
	  	return false;
	}
	jQuery(document).ready(function() {
		jQuery("#nbLoginForm_logindivmobile").find('input[name="login_email"]').keydown(function(event) {
