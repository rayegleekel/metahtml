<script>
    $(function () {
        $(".icon-search").on("click", function () {
            var sn = $("#text_Search").val();
            window.location.href = "/CList?sn=" + sn;
        });
        $(document).keydown(function (e) {
            var id = $(":text:focus").attr("id");
            if (e.keyCode == 13) {
                if (id == "text_Search") {
                    $(".icon-search").click();
                } else if (id == "text_Search1") {
                    $(".btn-search1").click();
                }
                return false;
            }
        });
        $(document).mouseup(function (e) {
            var _con = $('.arrow-top');//设置目标区域
            if (!_con.is(e.target) && _con.has(e.target).length === 0) {
                $('.arrow-top').hide();
            }
        });
    });
</script>
<script>
    function fastLogin(f, yy) {
        if (f == undefined || f == "") { f = ""; } else {
            f = encodeURIComponent(f)
        }
        if (yy == undefined || yy == "") {
            yy = "0";
        }   
