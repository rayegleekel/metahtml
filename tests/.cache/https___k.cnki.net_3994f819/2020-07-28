        $(function () {
            //$(".posr .icon-close1").click(function () {
            //    $(".banner-top").attr("href", "javascript:;");
            //    $(".banner-top").hide();
            //})
            $(".course-list .ht-l a").click(function () {

                if ($(this).attr("class") == "cur") {
                    return false;
                }
                else {
                    var index = $(".course-list .ht-l a").index(this);
                    $(".course-list .ht-l a").removeClass("cur");
                    $(this).addClass("cur");
                    $(".course-list .tul").css("display", "none");
                    $(".course-list .tul").eq(index).css("display", "block");
                }
            })

            $(document).keydown(function (e) {
                var id = $(":text:focus").attr("id");
                var pd = $(":password:focus").attr("id");
                if (e.keyCode == 13) {
                    if (id == "text_Search") {
                        $(".icon-search").click();
                    } else if (id == "Account" || pd == "Password") {
                        $(".login-btn").click();
                    }
                    return false;
                }
            });

            $(".fi-top .more").on("click", function () {
                var lk = $(".fi-top .cur").attr("data-link");
                window.open(lk);
            })
        })
