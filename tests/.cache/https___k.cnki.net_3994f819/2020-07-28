            layer.open({
                type: 2,
                scrollbar: false,
                title: ["用户登录","margin-top:30px;text-align: center;font-size: 24px;line-height: 2;background: white;margin-left:30px;"],
                content: '/Account/ECPLogin',
                area: ['542px', '480px'],
            });
        });
    }
    function weixinUrl() {
        var redirUrl = "http://k.cnki.net/account/WeiXinLogin";
        window.location.href = "http://my.cnki.net/ThirdLogin/ThirdLogin.aspx?to=weixin&RedirectUrl=" + escape(redirUrl);
    };

    function showimg(img) {
        layer.open({
            type: 1,
            title: "",
            area: ['800px', '510px'],
            maxmin: true,
            shadeClose: true, //开启遮罩关闭
            content: "<img src='" + img + "'>"
        });
    }
