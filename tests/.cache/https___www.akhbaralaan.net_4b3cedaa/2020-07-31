function calcSpeed(selector, speed) {
    // Time = Distance/Speed
    var spanSelector = document.querySelector(selector);
    if (spanSelector !== null) {
        var spanLength = spanSelector.offsetWidth;
        var timeTaken = spanLength / speed;
        spanSelector.style.animationDuration = timeTaken + "s";
    }
}

