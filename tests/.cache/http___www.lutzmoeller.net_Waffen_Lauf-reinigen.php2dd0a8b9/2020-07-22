	<script>
	function layoutHandler(){
		var CssLink = document.getElementById("Seite");
		var $w = document.getElementById('w'),
			$h = document.getElementById('h');
			if(window.innerWidth < 500){
			CssLink.setAttribute("href", "/1/small.css");
		} else if(window.innerWidth < 960){
			CssLink.setAttribute("href", "/1/medium.css");
		} else {
				CssLink.setAttribute("href", "/1/large.css");
		}
	}
	window.onresize = layoutHandler;
	layoutHandler();
	</script>
