<script>
        !function(w,d,s,u,p,t,o){
            w[p]=w[p]||function(){(w[p]._q=w[p]._q||[]).push(arguments)},
                t=d.createElement(s),o=d.getElementsByTagName(s)[0],
                t.async=1,t.src=u,o.parentNode.insertBefore(t,o)
        }(window, document, "script", "https://assets.poool.fr/poool.min.js", "poool");


        poool("init", "9RVUR-873D8-XUVC0-LASFT" );

        poool('config', 'debug', true);
        poool('style', 'brand_logo', 'https://nux3.tageblatt.lu/wp-content/themes/theme-tageblatt-v01/Ressources/images/Logo.svg');

        poool("config", "cookies_enabled", true);


        poool('config', 'user_is_premium', false);
        poool('config', 'login_url', "http://editpress-api.tageblatt.lu/login?backurl=" + window.location.href);

        //poool("config", "subscription_url", "http://mysite.com/subscribe?return_url=" + window.location.href);

        poool("send", "page-view", "premium");

        function onLock() {
  console.log('Article bloqué avec succès !');
}

function onRelease() {
  console.log('Article débloqué avec succès !');
  $(".custom-paywall").removeClass("custom-paywall");
}

function onError() {
  console.log('Article ERROR !');

}


// Pour écouter un évènement, il suffit d'appeler les fonctions 'event'
// ou 'on'
poool('event', 'onLock', onLock);
poool('on', 'onRelease', onRelease);

poool('event', 'onError', onError);
poool('config', 'wait_for_dom_load', true);

poool('event', 'onRegister', function(event) {
  //console.log('L\'utilisateur ' + event.email + ' s\'est enregistré à la newsletter ' + event.newsletter_id);

        var request = new XMLHttpRequest();
        request.open('GET', 'https://abo.tageblatt.lu/subscribeNewsletter?email=' + event.email, true);
        //'"' .  'email=' . event.email . '"'
        //var param = '"' +  'email=' + event.email + '"';
        request.send();

});


poool('event', 'onRelease', function(event) {
  //console.log('Contenu débloqué avec le widget ' + event.widget + (event.actionName ? ' nommé ' + event.actionName : ''));
  dataLayer.push({
      'event': 'poool_onRelease',
      'onReleaseSource' : event.widget
      });

});

poool('config', 'popover_timeout', 10000);



window.addEventListener('load', function () {

  setTimeout(function(){
    var url = location.href;
    var anchor = url.search('#');

    console.log(anchor);
    if(anchor != -1)
    {
      window.scrollTo(0,document.getElementById('survey').offsetTop);
      console.log(document.getElementById('survey').offsetTop);
    }
  }, 1000);


})


    </script>

    
