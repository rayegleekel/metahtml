            if (true) {
                $(".d_btn_mobile_page").show();
            }
        }

        // 모바일 페이지 전환
        $(".d_btn_mobile_page a").click(function () {
            $.cookie("viewMode", "mobile", {expires:1, path:"/", domain:".zum.com"});

            if (true) {
                location.href = "https://m.news.zum.com/articles/60508937";
            } else {
                location.href = "https://m.news.zum.com";
            }
        });
    })();
</script>
