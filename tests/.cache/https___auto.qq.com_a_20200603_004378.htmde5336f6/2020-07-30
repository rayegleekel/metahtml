<script>
	//汽车视频
	(function(){
		//var videoUrl = "http://data.v.qq.com/videocms/getNewsvideoList.php?ref=pclient&appkey=6UkwV9DeHr9_PC&vsite=new_vshou&ename=new_vs_auto&report=news.auto";
		// var videoUrl = "http://data.v.qq.com/videocms/getNewsvideoList.php?callback=infos&ref=pclient&appkey=6UkwV9DeHr9_PC&vsite=new_vshou&ename=new_vs3_auto";
		 var videoUrl = "//data.v.qq.com/videocms/getNewsvideoList.php?ref=pclient&appkey=6UkwV9DeHr9_PC&vsite=new_vshou&ename=new_vs3_auto";
		$.ajax({
			type: "get",
			async: false,
			url: videoUrl,
			dataType: "jsonp"
		}).done(function(json){
			if(json){
				var _data = json.data.content;
				var _html_1 = '';
				var _html_2 = '';
				//视频精选
				if(_data.length >= 3){
					for(var i=0; i<3; i++){
						_html_1 += '<li>';
						_html_1 += '	<a href="' + _data[i].link_addr + '" target="_blank">';
						_html_1 += '		<img src="' + _data[i].pic_url.replace(/^http[s]*:/,'') + '" alt="' + _data[i].title + '" />';
						_html_1 += '		<span></span><em></em>';
						_html_1 += '	</a>';
						_html_1 += '	<h4><a href="' + _data[i].link_addr + '" target="_blank">' + _data[i].title + '</a></h4>';
						_html_1 += '</li>';
					}
					$(".center-autoVideo-mod .bd ul").html(_html_1);
				}else{
					$(".center-autoVideo-mod").html('').hide();
					$(".right-autoVideo-mod").html('').hide();
					return false;
				}
				//汽车视频
				if(_data.length >= 7){
					for(var i=3; i<7; i++){
						_html_2 += '<div class="pt">';
						_html_2 += '	<div class="pic">';
						_html_2 += '		<a href="' + _data[i].link_addr + '" target="_blank">';
						_html_2 += '			<img src="' + _data[i].pic_url.replace(/^http[s]*:/,'') + '" alt="' + _data[i].title + '" width="120" height="70" />';
						_html_2 += '			<span class="playIcon"></span>';
						_html_2 += '		</a>';
						_html_2 += '	</div>';
						_html_2 += '	<div class="txt">';
						_html_2 += '		<h3><a href="' + _data[i].link_addr + '" target="_blank">' + _data[i].title + '</a></h3>';
						_html_2 += '		<p><a href="' + _data[i].link_addr + '" target="_blank">' + _data[i].subtitle + '</a></p>';
						_html_2 += '	</div>';
						_html_2 += '</div>';
					}
					$(".right-autoVideo-mod .bd .list").html(_html_2);
					//console.log(_data);
				}else{
					$(".right-autoVideo-mod").html('').hide();
					return false;
				}
			}else{
				$(".center-autoVideo-mod").html('').hide();
				$(".right-autoVideo-mod").html('').hide();
