<script type="text/javascript">
;(function(){
var TUBD_DATA_HTML = function(info){
        this._ID = $("#" + info.ID);
	this._URL = info.URL;
	this._NUM = info.NUM;
	this._HTML =info.HTML;
	this._FUN = info.FUN;
};
TUBD_DATA_HTML.prototype = {
	init:function(){
		var that = this,html = '',htmlNull = 0;
		that._HTML == "null" ? htmlNull = 0 : htmlNull = 1;
		$.ajax({
			type:"GET",
			url:that._URL,
			dataType:"jsonp",
			success:function(DATA){				
				if(DATA.code != 1){return false;};
				var len = 0;
				DATA.datanum >= that._NUM ? len = that._NUM : len = DATA.datanum;
				for(var i = 0;i < len;i++){
					if(that._HTML == "null"){					
						that._FUN(DATA.data[i],i,that._ID.children().eq(i));											
					}else{
						html = html + jQuery("<div>").append(that._FUN(DATA.data[i],i,$(that._HTML)).clone()).html();
					};
				}; 
				if(htmlNull == 1){that._ID.append(html)};
			}
		});		
	}
};
var TUBD_DATA_LIST_A = new TUBD_DATA_HTML({
	ID:'autoHotList',//回添ID
	URL:'//i.match.qq.com/pac/openapi?id=65',//TUBD数据
	NUM:4,//回添7条
	HTML:'<li><a target="_blank"></a></li>',//HTML
	FUN:function(data,idx,html){//回调方法					
		html.find("a").attr("href",data.url).html(data.title);		
		return html;//这里必须返回单条的HTML对象，主脚本集合后一次性回添到页面
	}
}).init();
})();



//左侧后三条
$.ajax({
  url:'//cgi.data.auto.qq.com/om/index.php?mod=om&act=autoarticle&num=3&refer=autopc&format=jsonp',
  dataType:'jsonp'
}).done(function(d){
  if(d.code==0){
	  var s = '';
	  for(var i=0;i< d.data.length;i++){
		  var item = d.data[i];
		  s+='<li><a target="_blank" href="'+item.url+'">'+item.title+'</a></li>';
	  }
	  $('#autoHotList3').html(s);
  }
});

</script><!--[if !IE]>|xGv00|715b51f6c1f0a8f89e49f6f6a4a3592a<![endif]--><!--[if !IE]>|xGv00|6ab3689cab5a51f0a9bef4f3cb346675<![endif]--><!--左侧栏2--><!--[if !IE]>|xGv00|e793c810f066f28912249303c00d0db6<![endif]-->
