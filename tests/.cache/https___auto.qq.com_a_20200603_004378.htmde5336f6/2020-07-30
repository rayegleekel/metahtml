<script>
<!--淘宝广告浮动-->
<!--include virtual="/public/final12/taobao_ad_js.htm" -->
<!--show_serial_message 车型库相关模板处理 汽车自研-->
seajs.use('//mat1.gtimg.com/auto/js/autoproject/autoarticle/show_serial_message-20160913', function(){
//if(qq.B.ie){return;}
//插件
var Widget = {};
//广告固定插件
Widget.scroll = function (obj, pos){
this.isFirst = true;
	this.y = qq.getY(obj);
	obj.parentNode.style.height = qq.height(obj) + 'px';
	this.scrollFun(obj);	
}

Widget.scroll.prototype = {
	scrollFun: function(obj){
		var foot = qq.GC(qq.G('Wrap'), '.foot-Article-QQ')[0];
		var _this = this, DH = 0, WH = 0, H = qq.height(foot) + qq.height(obj), SH = 0, MH = 0;
		
		qq.EA(window, 'scroll', function(){
			var t = qq.scrollY(), MH = qq.height(qq.G('Main-Article-QQ')), WH = qq.windowHeight();
			
			DH = document.body.clientHeight;
			SH = DH - WH;

			if(_this.y + qq.height(obj) > MH) {obj.style.cssText = 'position:static';};
//console.log('scrollHeight:'+t+'---objHeight:'+_this.y);
			if(t >= _this.y){
if(_this.isFirst){
					//console.log('beforeHeight:'+_this.y);
					_this.y = qq.getY(obj);
					_this.isFirst = false;
					//console.log('afterHeight:'+_this.y);
if(t < _this.y){return;}
				}
				if(t >= DH - WH) return;
				if(t > DH - H ){
					obj.style.position = 'absolute';
					obj.style.top = MH - qq.height(obj) + 'px';//Main-Article-QQ
				}else{
					obj.style.cssText = 'position:fixed; top:0';
				}
				if(qq.B.ie6){
					obj.style.position = 'absolute';
					a_global['ie6Fixed'](obj, 0);
				}
			}else{
				obj.style.cssText = 'position:static';
			}
			return false;
		});		
	}
	
}
//广告滚动浮动
qq.ready(function(){
	var taobao;
	setTimeout(function(){
		if(qq.G('F_ZQ_3')){
			taobao = qq.G('F_ZQ_3');
		}else if(qq.G('tips_F_Rectangle2')){
			taobao = qq.G('tips_F_Rectangle2');
		}else if(qq.G('ALLINONE_F_Button2')){
			taobao = qq.G('ALLINONE_F_Button2');
		}
		if(taobao){ new Widget.scroll(taobao) }
	}, 500);
});

});
</script>

