//<![CDATA[
	document.domain = 'qq.com';
	
	//_form.submit();
	var myLoaded = true;
	$(window).scroll(function(){
		if(($(window).scrollTop() >= $(".center-autoMall-mod").offset().top - $(window).height()) && myLoaded){
			
			function createPostContainer(_url,func,container,errFunc){
				var self = this;
				var oArea = (container && container.length) ? container : $(document.body);
				var oFrame = $('#_messagepost').size() ? $('#_messagepost') : $('<iframe id="_messagepost" name="_messagepost" style="display:none"></iframe>').appendTo(oArea);
				oFrame.unbind().bind('load',function(result){
					try{
						if(this.contentWindow.data){
							var oData = this.contentWindow.data;
							//console.log(oData);
							if(oData){
								typeof func === 'function' && func(oData);
							}else{
								if(typeof errFunc === 'function'){
									return errFunc(oData);
								}
							}
						}
						oFrame.remove();
					}catch(err){
						console.log(err);
					}
				});
				var _form = $('#_messageform');
					(_form && _form.size()) ?(function(){
						_form.attr('action',_url);
						_form.get(0).reset();
					})() : _form = $('<form action="'+ _url +'" method="post" target="_messagepost" id="_messageform" style="display:none;"></form>').appendTo(oArea);
				return _form;
			}
			var _form   = createPostContainer('//automall.qq.com/api/data/get_hot_list?callback=call',function(_data){
				//console.log(_data);
				var i=0;
				var tejiaLi = "";
				for(var o in _data){
					if(i<3){
						tejiaLi += '<li>'
						tejiaLi += '<a href="' + _data[o].web_url /*+ '&engine_id=' + _data[o].id*/ + '" target="_blank">'
						tejiaLi += '<img src="' + _data[o].cover_img.replace(/^http[s]*:/,'') + '" alt="' + _data[o].series_name + _data[o].model_name + '">'
						tejiaLi += '</a>'
						switch(_data[o].card_type){
							//case "shangou ":
							//	tejiaLi += '<em class="sg"></em>';
							//	break;
							case "card_youhui":
								tejiaLi += '<em class="yh"></em>';
								break;
							case "card_daijin":
								tejiaLi += '<em class="dj"></em>';
								break;
							case "card_zhekou":
								tejiaLi += '<em class="zk"></em>';
								break;
							case "card_test_drive":
								tejiaLi += '<em class="sj"></em>';
								break;
						}
						tejiaLi += '<h4><a href="' + _data[o].web_url /*+ '&engine_id=' + _data[o].id*/ + '" target="_blank" title="' + _data[o].name +'">' + _data[o].name +'</a></h4>';
						
						if(_data[o].card_type == "card_test_drive"){
							tejiaLi += '<h5><strong>' + _data[o].buy_price + '</strong>元</h5>';
						}else{
							tejiaLi += '<h5><strong>' + _data[o].mall_price_million + '</strong>万 <span>' + _data[o].market_price_million + '</span></h5>';
						}

						tejiaLi += '</li>';
						//console.log(_data[o].mall_price_million.match(/[\u4e00-\u9fa5],{0,}/g);
					}else{
						break;
					}
					i++;
				}
				$(".center-autoMall-mod .bd ul").html(tejiaLi);
			});
			_form.submit();
			
			myLoaded = false;
		}
	});
//]]>
</script>
