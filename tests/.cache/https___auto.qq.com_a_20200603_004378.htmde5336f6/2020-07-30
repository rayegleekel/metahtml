function gdtad(gdtAD1, gdt_id1, gdtAD2, gdt_id2){
	GDT.get([[gdtAD1, document.getElementById(gdt_id1), 1, false, function(o) {
		// o.ret 表示没有广告， 这里可以开发同学随便改
		if (!o.ret) {
			document.getElementById(gdt_id1).style.display = 'none';
		    document.getElementById("cgd").style.height = '80px';
	    	document.getElementById("gdt_right").style.display = 'block';
	    	document.getElementById("gdt_left").style.display = 'block';
            return;
		}
		if ( o.data.length == 1 ) { 
	 		document.getElementById("gdt_right").style.display = 'block';
	  
		} 
	}, {  count: 2 }]
	]); 
}
