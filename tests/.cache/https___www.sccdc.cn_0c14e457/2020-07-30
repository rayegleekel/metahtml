                        <script language="javascript">
                            var speed = 150;
                            $(demoup2).html($(demoup1).html());
                            function Marquee2() {

                                if ($(demoup2).height() - $(demoup).scrollTop() <= 0) {
                                    $(demoup).scrollTop(-$(demoup1).outerHeight());
                                }
                                else {
                                    $(demoup).scrollTop($(demoup).scrollTop() + 1);
                                }
                            }
                            var MyMar = setInterval(Marquee2, speed);
                            $(demoup).hover(function () {
                                clearInterval(MyMar)
                            }, function () {
                                MyMar = setInterval(Marquee2, speed)
                            });
                        </script>

