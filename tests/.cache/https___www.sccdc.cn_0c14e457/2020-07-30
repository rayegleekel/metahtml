    <script type="text/javascript">
        function callScroll(Sid, Stop) {
            $(Sid).stop();
            var scrollTop = $(document).scrollTop();
            $(Sid).css({
                "bottom": -(scrollTop - Stop),
            });
        }
        function So(Sid) {
            //$(Sid).stop();
            var scrollTop = $(document).scrollTop();
            $(Sid).css({
                "top": scrollTop + 300,
            });
        }
    </script>


    
    <script type="text/javascript">
        var i = 2;
        $(window).ready(function () {
            $("#a1").mouseenter(function () {
                i = 1;
            });
            $("#a2").mouseenter(function () {
                i = 2;
            });
            $("#a3").mouseenter(function () {
                i = 3;
            });
            setInterval(function () { $("#a" + i).mouseenter(); i++; if (i == 4) i = 1; }, 4000);
        });
    </script>
