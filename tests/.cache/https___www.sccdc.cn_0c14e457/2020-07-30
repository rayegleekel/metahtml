                <script type="text/javascript">
                    $(".index_Center_news").each(function () {
                        var $this = $(this);
                        $(this).find(".Center_txt").each(function (i) {
                            $(this).hover(function () {
                                $this.find(".Center_select").removeClass("Center_select");
                                $(this).addClass("Center_select");
                                $this.find(".Center_info:visible").hide();
                                $this.find(".Center_info:eq(" + i + ")").show();
                            }, function () { });
                        }).eq(0).mouseenter();
                    });


