//一级导航“互动”链接表配置
var navCitysArr = [
    { name: "南京", en: "nanjing" },
    { name: "沈阳", en: "shenyang" },
    { name: "大连", en: "shenyang" },
    { name: "鞍山", en: "shenyang" },
    { name: "抚顺", en: "shenyang" },
    { name: "本溪", en: "shenyang" },
    { name: "丹东", en: "shenyang" },
    { name: "锦州", en: "shenyang" },
    { name: "营口", en: "shenyang" },
    { name: "阜新", en: "shenyang" },
    { name: "辽阳", en: "shenyang" },
    { name: "盘锦", en: "shenyang" },
    { name: "铁岭", en: "shenyang" },
    { name: "朝阳", en: "shenyang" },
    { name: "葫芦岛", en: "shenyang" }
];


loadScript("//fw.qq.com/ipaddress",function(){
    // var IPData = new Array("113.141.119.105","","陕西省","安阳市");
    for(var city_i = 0; city_i < navCitysArr.length; city_i++){
        if ((IPData[3] == '' && IPData[2].split("市")[0] == navCitysArr[city_i].name) || (IPData[3] != '' && IPData[3].split("市")[0] == navCitysArr[city_i].name)) {
            document.getElementById("ipClub").setAttribute("href" , "//" + navCitysArr[city_i].en + ".weche.club");
        }
    }
});

function loadScript(url,callback){
    var script = document.createElement("script");
    script.type = "text/javascript";
    if(script.readyState){ //IE
        script.onreadystatechange = function(){
            if(script.readyState == "loaded" || script.readyState == "complete"){
                script.onreadystatechange = null;
                callback();
            }
        };
    }else{ //其他浏览器
        script.onload = function(){
            callback();
        }
    }
    script.src = url;
    document.getElementsByTagName("head")[0].appendChild(script);
}
</script><!--[if !IE]>|xGv00|1be646953161e581d9484d40167f2b63<![endif]-->    </div>
