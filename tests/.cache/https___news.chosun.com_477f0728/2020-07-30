      <script>
        function edito_roll() {
          var now = new Date();
          var hour = now.getHours();
          if (hour.toString().length == 1) {
            var hour = '0' + hour;
          }
          var edito = document.querySelectorAll('#opi_01_id .news_item');
          var edito_count = edito.length;
          if (edito_count == 3) {
            //console.log('3ê°ë¤ì!');
            if (hour % 3 == 0) {
              edito[0].style.display = "block";
              edito[1].style.display = "none";
              edito[2].style.display = "none";
            } else if (hour % 3 == 1) {
              edito[0].style.display = "none";
              edito[1].style.display = "block";
              edito[2].style.display = "none";
            } else if (hour % 3 == 2) {
              edito[0].style.display = "none";
              edito[1].style.display = "none";
              edito[2].style.display = "block";
            } else {
              return false;
            }
          } else if (edito_count == 2) {
            //console.log('2ê°ë¤ì!');
            if (hour % 2 == 0) {
              edito[0].style.display = "block";
              edito[1].style.display = "none";
            } else if (hour % 2 == 1) {
              edito[0].style.display = "none";
              edito[1].style.display = "block";
            } else {
              return false;
            }
          } else if (edito_count == 1) {
            //console.log('1ê°ë¤ì!');
            edito[0].style.display = "block";
          } else if (edito_count >= 4) {
            //console.log('4ê°ê° ëë¤ì!');
            for (var i = 0, length = edito_count; i < length; i++) {
              edito[i].style.display = "none";
              edito[0].style.display = "block";
            }
          } else {
            return false;
          }
        }

        function edito_roll2() {
          var now = new Date();
          var hour = now.getSeconds();
          if (hour.toString().length == 1) {
            var hour = '0' + hour;
          }
          var edito = document.querySelectorAll('#opi_02_id .news_item');
          var edito_count = edito.length;
          if (edito_count == 3) {
            //console.log('3ê°ë¤ì!');
            if (hour % 3 == 0) {
              edito[0].style.display = "none";
              edito[1].style.display = "table-cell";
              edito[2].style.display = "none";
            } else if (hour % 3 == 1) {
              edito[0].style.display = "none";
              edito[1].style.display = "none";
              edito[2].style.display = "table-cell";
            } else if (hour % 3 == 2) {
              edito[0].style.display = "table-cell";
              edito[1].style.display = "none";
              edito[2].style.display = "none";
            } else {
              return false;
            }
          } else if (edito_count == 2) {
            //console.log('2ê°ë¤ì!');
            if (hour % 2 == 0) {
              edito[0].style.display = "none";
              edito[1].style.display = "table-cell";
            } else if (hour % 2 == 1) {
              edito[0].style.display = "table-cell";
              edito[1].style.display = "none";
            } else {
              return false;
            }
          } else if (edito_count == 1) {
            //console.log('1ê°ë¤ì!');
            edito[0].style.display = "block";
          } else if (edito_count >= 4) {
            //console.log('4ê°ê° ëë¤ì!');
            for (var i = 0, length = edito_count; i < length; i++) {
              edito[i].style.display = "none";
              edito[1].style.display = "table-cell";
            }
          } else {
            return false;
          }
        }

        //edito_roll()
        edito_roll2()
      </script>

