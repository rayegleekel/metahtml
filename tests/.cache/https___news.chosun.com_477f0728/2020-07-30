            <script type="text/javascript">
            function _getRankingList() {
              var _catid = arguments.length > 0 ? arguments[0] : ""
                , _url = "/ranking/rss/index/index.xml"
                , _tot = 5
                , _exp = new RegExp("http://[^>]*(site/data|sitedata)/(img_dir|thumb_dir|image|thumbnail)[^>]*_thumb\\.jpg", "gim");
              if (_catid != '') _url = "/ranking/rss/" + _catid + "/index/index.xml";
              if (_catid == 'hot') _tot = 10;
              var _icon = "view icon1";
              if (_catid == "hot") _icon = "";
              else if (_catid == "snslike") _icon = "view icon2";
              else if (_catid == "sns") _icon = "view icon3";
              else if (_catid == "bbs") _icon = "view icon4";
              $.ajax({
                url: _url
                , type: 'GET'
                , async: true
                , dataType: 'xml'
                , success: function(resultXML){
                  try {
                    var itemList = $(resultXML).find("item");
                    if (itemList == null || itemList.length == 0) return;
                    var tot = itemList.length < _tot ? itemList.length : _tot; 

                    var idx = 0, htmlstr = '';
                    for(var no=0; no<itemList.length; no++){

                      var title_element = $(itemList[no]).children("title")
                        , link_element = $(itemList[no]).children("link")
                        , thumb_element = $(itemList[no]).children("comments")
                        , desc_element = $(itemList[no]).children("description");
                      if (title_element == null || link_element == null || thumb_element == null) continue;

                      var _title = $(title_element).text()
                        , _link = $(link_element).text()
                        , _thumb = $(thumb_element).text()
                        , _desc = $(desc_element).text()
                        , _hit = "0";

                      var exp = /(\s)(total:)([\d]+)(\s)/;
                      if (_catid == 'bbs') exp = /(\s)(bbstotal:)([\d]+)(\s)/;
                      var tmp = _desc.match(exp);
                      if (tmp != null && tmp.length > 3) _hit = tmp[tmp.length-2];

                      if (_catid == 'hot') {
                        htmlstr += '<li><a href="' + _link + '">' + _title + '</a></li>';
                      }
                      else { 
                        htmlstr += '<div class="tab_link">'
                          + '<dl>'
                          + '<dt><span>' + (idx+1) + '</span><a href="' + _link + '">' + _title + '</a></dt>'
                          + '<dd class="num_more"><span class="' + _icon + '"></span>' + _hit + '</dd>'
                          + '</dl>';
                        if (_thumb != '') {
                          htmlstr += '<span class="step_thumb">'
                            + '<a href="' + _link + '"><img src="' + _thumb + '" onerror="this.style.display=\'none\';"></a>'
                            + '</span>';
                        }
                        htmlstr += '</div>';
                      }

                      idx++;
                      if (idx >= tot) break;
                    }

                    var _layer = $(".hot_lank_area .tab_cont");
                    if (_catid == "hot") $(_layer).children("div").eq(0).find("ul").html(htmlstr);
                    else if (_catid == "snslike") $(_layer).children("div").eq(1).html(htmlstr);
                    else if (_catid == "sns") $(_layer).children("div").eq(2).html(htmlstr);
                    else if (_catid == "bbs") $(_layer).children("div").eq(3).html(htmlstr);
                  }
                  catch (e) { }
                }
                , error: function(status, error) {
                }
              });
            }
            //$(document).ready(function(){
            _getRankingList('hot') ;
            _getRankingList('snslike') ;
            _getRankingList('sns') ;
            _getRankingList('bbs') ;
            //});
            </script>
