    <script type="text/javascript">
    $(function () {
        // if (detectmob) {
        //     $('.ad').hide();
        //     $('.ad_mo').show();
        // } else {
        //     $('.ad').show();
        //     $('.ad_mo').hide();
        // }

		//pageview
        $.post('/api/recordProgramViewsCount', {programid: 14, type: "medical", id: 323465, tbl: 'article'});

        var size_p = $('.detail_div p').first().css('font-size'),
        size_h2 = (parseInt(size_p.substr(0,2))+6)+'px',
        size_h3 = (parseInt(size_p.substr(0,2))+2)+'px',
        size_h4 = (parseInt(size_p.substr(0,2))+4)+'px';

        $('.detail_div p').css('font-size', size_p);
        $('.detail_div h2').css('font-size', size_h2);
        $('.detail_div h3').css('font-size', size_h3);
        $('.detail_div h4').css('font-size', size_h4);

        /*--字體放大--*/
        // var text_p=18,text_h2=24,text_h3=20,text_h4=22;
        var text_p = parseInt($('.detail_div p').first().css('font-size').substr(0,2)),
        text_h2 = parseInt(text_p+6),
        text_h3 = parseInt(text_p+2),
        text_h4 = parseInt(text_p+4);

        $('.txt_scale').click(function() {

            if(text_p<24) {
                text_p+=2;
                $('.detail_div p').css({fontSize:+text_p+'px'});
            } else {
                text_p=18;
                $('.detail_div p').css({fontSize:+text_p+'px'});
            }

            if(text_h2<30) {
                text_h2+=2;
                $('.detail_div h2').css({fontSize:+text_h2+'px'});
            } else {
                text_h2=24;
                $('.detail_div h2').css({fontSize:+text_h2+'px'});
            }

            if(text_h3<26) {
                text_h3+=2;
                $('.detail_div h3').css({fontSize:+text_h3+'px'});
            } else {
                text_h3=20;
                $('.detail_div h3').css({fontSize:+text_h3+'px'});
            }

            if(text_h4<28) {
                text_h4+=2;
                $('.detail_div h4').css({fontSize:+text_h4+'px'});
            } else {
                text_h4=22;
                $('.detail_div h4').css({fontSize:+text_h4+'px'});
            }
            // console.log(text_p);
        });

        /*--字體放大ed--*/
        /*img加上圖說*/
        // $('.detail_div img').each(function() {
        //     // var target = $( event.target );
        //     if (!$(this).parent().next().hasClass('img_alt')) {
        //         $(this).parent().after('<p style=text-align:center class="img_alt"><small>'+$(this).attr('alt')+'</small></p>')
        //     }
        //
        // });
        /* 顯示隱藏line friend banner */
        // if(detectmob()) {
        //     $('.line_friend').hide();
        //     $('.line_friend_mo').show();
        // } else {
        //     $('.line_friend_mo').hide();
        //     $('.line_friend').show();
        // }
        /* 移除內文img style*/
        $('.detail_div img').attr('style', 'none');
        //啟動lazyload
        $('img.lazyimage').lazyload();

    });



    </script>
