$("#mobileHeader .right").click(function(){
		var $_this = $(this),
			$_mobileNavList = $("#mobileHeader .mobileNavList");
		if(!$_this.hasClass("open")){
			$_this.addClass("open");
			$_mobileNavList.animate({height:520}, 200);
		}else{
			$_this.removeClass("open");
			$_mobileNavList.animate({height:0}, 200);
		}
	});
