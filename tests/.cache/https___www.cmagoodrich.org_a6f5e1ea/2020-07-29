<script>
$(document).ready(function(e) {
	$("#frm_login").validator();
	$("#frm_login #btn-login").click(function(e){
		e.preventDefault();
		$("#frm_login").submit();
	});
	$("#frm_login input").keypress(function(e){
		if(e.keyCode==13){
			$("#frm_login").submit();
		}
	});
	$("#btn-nav-login").click(function(e){
		e.preventDefault();
		if($("#frm-login").hasClass("opened")){
			$("#frm-login").removeClass("opened").stop(true,true).slideUp();
		}else{
			$("#frm-login").addClass("opened").stop(true,true).slideDown();
			$("#nav-menu").removeClass("opened").stop(true,true).slideUp();
		}
	});
	$("#btn-nav-menu").click(function(e){
		e.preventDefault();
		if($("#nav-menu").hasClass("opened")){
			$("#nav-menu").removeClass("opened").stop(true,true).slideUp();
		}else{
			$("#nav-menu").addClass("opened").stop(true,true).slideDown();
			$("#frm-login").removeClass("opened").stop(true,true).slideUp();
		}
	});
})
</script></body>
</html>
