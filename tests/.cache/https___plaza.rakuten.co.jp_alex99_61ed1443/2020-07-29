<script type="text/javascript"><!--
// 他のブログを見る
var setAnotherBlogLink = function(){
  var user = diary_global_header_user;
  var rndurl = new String();
  var gau = document.getElementById("diary_global_header_another");
  if(!gau) return;
  rndurl += "http://plaza.rakuten.co.jp/rnd/";
  rndurl += user;
  rndurl += "/";
  gau.href = rndurl;
}
setAnotherBlogLink();
// ジャンルリンク
var setGenreLink = function(){
  var genreid = diary_global_header_genreid;
  var genrename = diary_global_header_genrename;
  var giu = document.getElementById("diary_global_header_icon"); 
  if(!giu) return;
  var gnrurl = new String();
  gnrurl += "/gnr/g";
  gnrurl += genreid;
  gnrurl += "/";
  var gnrcls = new String();
  gnrcls += "icon";
  gnrcls += genreid;
  giu.href = gnrurl;
  giu.className = gnrcls;
  giu.innerHTML = genrename;
}
setGenreLink();
// グローバルヘッダ高さ
var createGHeader = setInterval(function(){
  var headerAd = document.getElementById("headerAd");
  if(!headerAd) return;
  var content_height = headerAd.clientHeight;
  var ghb = document.getElementById("global_height_box");
  if(!ghb) return;
  var pix = new String();
  pix += content_height;
  pix += "px";
  ghb.style.height = pix;
}, 0);
//-->
</script>
