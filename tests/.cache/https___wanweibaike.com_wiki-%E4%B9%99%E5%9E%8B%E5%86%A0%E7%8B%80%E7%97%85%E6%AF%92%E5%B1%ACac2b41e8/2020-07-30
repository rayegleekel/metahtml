var autoUpdate="";
  $('#updateNow').click(function(){
    var updateUrl="/Article/update";
    var id=$(this).data('id');
    var my=$(this);
    var wait=15;
    timer = setInterval(function(){
        if(wait==1){
            my.removeClass('disabled').html('更新本页');
            clearInterval(timer);
            location.reload();
        }else{
            wait--;
            my.addClass('disabled').html('正在更新:'+wait);
        }
    },1000);
    $.post(updateUrl,{id:id},function(data){
        if(data == 1){
          clearInterval(timer);
          my.text('更新太频繁，两分钟之后再试');
          if(!autoUpdate){
          	alert('更新太频繁，两分钟之后再试');
          }
          return false;
        }else if(data == 2){
          location.reload();
        }else if(data == 4){
          alert('已经是最新内容，请明天再更新');
          clearInterval(timer);
        }else{
          alert('更新失败，请两分钟之后再试');
        }
    },'json');
  })
  
