  $('table.navbox-inner tbody').each(function(i,n){
    var firstTr=$(this).children(":first");
    firstTr.addClass('default-hide');
    firstTr.siblings().addClass('hidden');
    firstTr.click(function(){
      var thisTr=$(this);
      if(thisTr.hasClass('default-hide')){
        thisTr.siblings().removeClass('hidden');
        thisTr.siblings().each(function(){
          var trSiblings=$(this);
          trSiblings.find('tr').removeClass('hidden');
        });
        thisTr.removeClass('default-hide');
      }else{
        thisTr.siblings().addClass('hidden');
        thisTr.find('tr').addClass('hidden');
        thisTr.addClass('default-hide');
      }
    }); 
  });
