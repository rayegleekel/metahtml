	for (var word in mobileKeyWords){
		if (navigator.userAgent.match(mobileKeyWords[word]) != null){			
			if (location.href != 'http://weekly.cnbnews.com/?mobile') {
				location.href = 'http://m.weekly.cnbnews.com/m/';
				break;
			}
	    }
    }
