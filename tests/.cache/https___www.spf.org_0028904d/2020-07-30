    <li><a href="#" class="btn_share" data-sns="ht"><img src="/spf-china-observer/images/btnShareHatena@2x.png" alt="はてな" width="80"></a></li>
  </ul></div>
</div><script>
/*<![CDATA[*/
//
(function($){
try{ (function($){
  // SNSボタンクリック時に各SNSに遷移
  var enc_now_url = encodeURIComponent(location.href); //現在ページをURLエンコード
  var link_url = null;  // 遷移先SNS URL
  var win_opt = 'width=650,height=450,menubar=no,toolbar=no,scrollbars=yes';  //ウィンドウを開くときのオプション
  
  $(document).on('click', '.btn_share', function() {
    var sns = $(this).attr('data-sns');
    switch(sns) {
      case "fb":
        // facebook
        link_url = "https://www.facebook.com/share.php?u=" + enc_now_url;
      break;
      case "tw":
        // twitter
        link_url = "https://twitter.com/share?url=" + enc_now_url;
      break;
      case "ln":
        // line
        link_url = "http://line.me/R/msg/text/?" + enc_now_url;
      break;
      case "ht":
        //hatena blog
        link_url = "http://b.hatena.ne.jp/add?url=" + enc_now_url;
      break;
    }
    window.open(link_url, 'FBwindow', win_opt);
    return false;
  });
  
  
  //サイドバーからサイト下部に移動
  var url = location.href;
  if (url.indexOf("sidelayout") != -1) {
    console.log('admin page.'); //管理画面のサイドバー編集ページ　ではDOM削除実行しない
  } else {
    trg_html = $('.sns_wrap').html();
    $('.sns_wrap').hide();
    if($('.asideArea').length == 0) {
      $('.document').after('<div class="asideArea"></div>');
    }
    $('.asideArea').prepend(trg_html);
  }
})(jQuery); } catch(e) { console.error(e); }
})(jQuery);
//
/*]]>*/
</script>
