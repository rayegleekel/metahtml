  <ul></ul></div><script>
/*<![CDATA[*/
//
(function($){
try{ $(function() {
  var noArticles = 6  // 出力する記事数
  $.ajax({
    url: 'https://feed.meltwater.com/gyda/outputs/xml/html/16/92/8/v2_477451.html.XML',
    dataType: 'xml',
    error: function() {
      $("div.list_media ul").append('<div class="title"><a href="#" target="_blank">...</a></div>');
    },
    success: function(xml) {
      $count = 0;
      $(xml).find("document").each(function() {
        $count++;
        if ($count > noArticles) {
            return false;
        }
        $("div.list_media ul").append('<li><div class="bCatListLabel"><span class="bcatlabel">' +
          $(this).find('sourcename').text() +
          '</span></div><p class="bCatListDate">' +
          $(this).find('createDate_year').text() +
          '.' +
          $(this).find('createDate_mon2').text() +
          '.' +
          $(this).find('createDate_day2').text() +
          '</p><h5>' +
          '<a href="' +
          $(this).find('url').text() +
          '" target="_blank">' +
          $(this).find('title').text() +
          '</a></h5></li>');
      });
    }
  });
}); } catch(e) { console.error(e); }
})(jQuery);
//
/*]]>*/
</script>
</div>
