            var last = false;
            var lock = false;
            var timer;
            var page = 2;
            //var ad_item = 1;
            $(function () {
                $('.lazyimage').lazyload();
                //總覽列表 無限瀑布流 移到最下面才會啟動
                $(window).scroll(function () {
                    var document_height = $(document).height();
                    var window_height = $(window).height();
                    var window_scrolltop = $(window).scrollTop();
                    // var page = $('#page').val();
                     // $('.lazyimage').lazyload();
                    console.log('scroll');
                    console.log('lock'+lock);
                    console.log('last'+last);
                    if (window_scrolltop >= document_height - window_height - 300) {
                        if (!last && !lock) {
                            lock = true;
                            if (timer)
                                clearTimeout(timer);
                            timer = setTimeout(function () {
                                $.get("/cate/LoadMoreOverview/medical/" + page, function (data) {
                                    console.log('data'+data);
                                    if (data == '[]') {
                                        last = true;
                                        return;
                                    }
                                    lock = false;
                                    data_array = JSON.parse(data);
                                    var dataTemplate = combolistTemplate(data_array);
                                    $('#combolistUl').append(dataTemplate);
                                    // $('#page').val(parseInt(page) + 1);
                                    page++;
