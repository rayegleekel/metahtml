            function combolistTemplate(data) {
                var html = '';

                for (i = 0; i < data.length; i++) {
                    var link = data[i]['url'];
                    var image = data[i]['cover_image'];
                    var title = data[i]['title'];
                    var publish = data[i]['publish'];
                    var content = data[i]['content'];
                    var tags = data[i]['tag'].split(',');
                    var video_id = data[i]['video_id'];
                    var lazy_image = "https://cc.tvbs.com.tw/2017program/health/images/health_default.jpg";

                    html += '<li>';
                    html += '<a href="'+link+'">';
                    html += '<div class="img"><img class="lazyimage" data-original="'+image+'" src="'+lazy_image+'"></div>';
                    html += '<div class="txt_box">';
                    html += '<div class="title">';
                    if (video_id) {
                        html += '<div class="play"></div>';
                    }
                    html += '<span>'+title+'</span></div>';
                    html += '<div class="txt"><span>'+content+'</span></div>';
                    html += '</div>';
                    html += '</a>';
                    html += '<div class="tag">';
                    if(tags != null && tags != ''){
                        tags.forEach(function(v,k){
                            if(k>=2) return false;
                            html += '<p><a href="javascript:searchTag(\'14\',\''+encodeURI(v)+'\')">'+v+'</a></p>';
                        });
                    }else{
                        html += "<p><a style='background: rgba(0,0,0,0)' href='javascript: void(0);'></a></p>";
                    }

                    html += '</div>';
                    html += '</li>';

                    if((i+1)%4 == 0){
                        //廣告
                        // html += "<li>";
                        // html += "<div class='ad'>";
                        // html += "<a href='#'><img src='http://campaign.tvbs.com.tw/test/flyzero/2020health/images/admo2.jpg' alt=''/></a>";
                        // //html += "<div id='v4_health_desktop_index_superrec_R"+ad_item+"' data-adsid='v4_health_desktop_index_superrec_R"+ad_item+"'>";
                        // html += "</div>";
                        // html += "<div class='ad_mo'>";
                        // html += "<a href='#'><img src='http://campaign.tvbs.com.tw/test/flyzero/2020health/images/admo2.jpg' alt=''/></a>";
                        // html += "</div>";
                        // html += "</li>";
                        //ad_item++;
                    }

                }
                return html;
            }
        </script>

