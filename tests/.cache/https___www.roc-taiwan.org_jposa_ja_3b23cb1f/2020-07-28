<div class="top-button-wrap">
	<img src="/imgAssets/version2/Top-Button-240.png" />
</div>

<script>

	var topButtonWrap = $(".top-button-wrap");
	var htmlWrap = $("html");

	topButtonWrap.click(function(e){
		e.preventDefault();
		htmlWrap.animate({ scrollTop:"0" }, 800, "easeInSine");
	});

	$(window).scroll(function(){
		if(htmlWrap.scrollTop() >= 600){
			topButtonWrap.fadeIn();
		}else{
			topButtonWrap.fadeOut();
		}
	})

</script>



