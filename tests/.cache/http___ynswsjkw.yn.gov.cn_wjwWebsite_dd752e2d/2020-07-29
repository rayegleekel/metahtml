<script type="text/javascript">
	$(function() {

		//浏览器版本检查
		browserCheck();
		
		setGov();

		$('#owl-demo').owlCarousel({
			items : 1,
			navigation : true,
			navigationText : [ "上一个", "下一个" ],
			autoPlay : true,
			stopOnHover : true
		}).hover(function() {
			$('.owl-buttons').show();
		}, function() {
			$('.owl-buttons').hide();
		});
	});
	function setTab(name, cursel, n) {
		for (var i = 1; i <= n; i++) {
			var menu = document.getElementById(name + i);
			var con = document.getElementById("con_" + name + "_" + i);
			menu.className = i == cursel ? "hover" : "";
			con.style.display = i == cursel ? "block" : "none";
		}
	}
	
	function mydefault4xwfb(){
		document.getElementById("con_one_0").style.display="none";
	}
	
	function mydefault3tzgg(){
		document.getElementById("con_two_0").style.display="none";
	}
	
	function mydefault4tzgg(){
		document.getElementById("con_three_0").style.display="none";
	}
	
	
	
	//国务院文件和省政府文件
	function setGov(){
		try {
			$(".lmsj").children().each(function(index){ 
				if(index<=6){
					var html="<li>"+$(this).html()+"</li>";
					$("#shengzhengfu").append(html);
				}
			});
		} catch (e) {}
		
		try {
			$(".govpushinfo150203").children().each(function(index){ 
				if(index<=6){
					var html="<li>"+$(this).html()+"</li>";
					$("#guowuyuan").append(html);
					$("#guowuyuan_first").append(html);
				}
			});
		} catch (e) {}
	}
	
</script>
