  <% for (var i = 0; i < rc.posts.length; i++) { %>
    <% var iter = rc.posts[i] %>
    <li class="articleList__item">
      <a class="articleList__link" href="/<%=iter.id%>">
        <% if (iter.cover_image) { %>
          <div class="articleList__imgWrapper" style="background-image:url(<%=iter.cover_image.thumb_360%>)"></div>
        <% } else if (iter.images && iter.images.length > 0) { %>
          <div class="articleList__imgWrapper" style="background-image:url(<%=iter.images[0].thumb_360%>)"></div>
        <% } else if (iter.unit.profile_image) { %>
          <div class="articleList__imgWrapper articleList__imgWrapper--unitImg" style="background-image:url(<%=iter.unit.profile_image.square_200%>)"></div>
        <% } else {%>
          <div class="articleList__imgWrapper articleList__imgWrapper--noImg"></div>
        <% } %>
        <div class="articleList__titleWrapper">
          <% var titleWithSubtitleClass = ! iter.subtitle && "articleList__title--noSub" %>
          <h2 class="articleList__title <%=titleWithSubtitleClass%>"><%=iter.title%></h2>
          <% if (iter.subtitle) { %>
            <p class="articleList__subTitle"><%=iter.subtitle%></p>
          <% } %>
          <% if (iter.description) { %>
            <p class="articleList__description"><%=iter.description%></p>
          <% } %>
          <div class="articleList__subWrapper">
            <p class="articleList__subText"><%=iter.unit.name%></p>
          </div>
        </div>
      </a>
      <% var labels = rc.getLabels(iter.labels) %>
      <% var autoLabels = rc.getAutoLabels(iter.labels, iter.auto_labels) %>
      <% var totalLabel = labels.length + autoLabels.length %>
      <% if (totalLabel > 0) { %>
        <div class="articleList__property">
          <div class="articleList__propertyItem">
            <ul class="labelList">
              <% var unitParam = '?unit=' + rc.posts[i].unit.id + '&all=true' %>
              <% for (var j = 0; j < labels.length; j++) { %>
                <% var label = labels[j] %>
                <li class="labelList__item">
                  <a class="labelList__link labelList__link--ch" href="/-/labels/<%=label%><%=unitParam%>">
                    <p class="labelList__name">#<%=label%></p>
                  </a>
                </li>
              <% } %>
              <% for (var j = 0; j < autoLabels.length; j++) { %>
                <% var label = autoLabels[j] %>
                <li class="labelList__item">
                  <a class="labelList__link" href="/-/labels/<%=label%><%=unitParam%>">
                    <p class="labelList__name">#<%=label%></p>
                  </a>
                </li>
              <% } %>
            </ul>
          </div>
        </div>
      <% } %>
    </li>
  <% } %>

</script>

