  <script>
  (function($) {
    'use strict'
    const headerType = $('.js-variableHeader').data('header-type'),
          $hiddenHeader = $('.js-variableHeader'),
          $profileImg = $('.js-variableHeader__authorImg'),
          $profileAlterImg = $('.js-variableHeader__alterImg'),
          $labelTitle = $('.js-variableHeader__labelTitle');

    $(window).scroll(function() {
      let scrollTop = $(window).scrollTop();

      if (headerType == 'author') {
        const switchingPoint = 55,
              beforeImgSize = 80,
              afterImgSize = 40,
              afterAlterImgSize = 28;

        if (scrollTop > switchingPoint + beforeImgSize) {
          $hiddenHeader.css({'display': 'flex'});
          $profileImg.width(afterImgSize);
          $profileImg.height(afterImgSize);
          $profileAlterImg.width(afterAlterImgSize);
          $profileAlterImg.height(afterAlterImgSize);
        } else {
          $hiddenHeader.hide();
        }
      }

      if (headerType == 'label') {
        let labelTitlePosition = $labelTitle.position().top;
        const switchingPoint = labelTitlePosition - 11;
        if (scrollTop > switchingPoint) {
          $hiddenHeader.css({'display': 'flex'});
        } else {
          $hiddenHeader.hide();
        }
      }
    });
  })(jQuery);
  </script>



