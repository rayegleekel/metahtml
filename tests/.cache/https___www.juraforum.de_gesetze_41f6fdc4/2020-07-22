                            <script>$jfjQ(document).ready(function(){$jfjQ('.jf-desk-top').on("mouseenter",function(){window.desk_is_hovered=1;});$jfjQ('.jf-desk-top').on("mouseleave",function(){window.desk_is_hovered=0;});$jfjQ('.jf-bereiche nav ul li').on("mouseenter",function(){checkIfMouseHover($jfjQ(this));});setInterval(function(){checkCloseMenu();},500);window.subnavi_std=$jfjQ(".jf-navigation").html();});function checkIfMouseHover(elem){var klasse=elem.attr('class');loadSNL(elem);var add_position=0;if(klasse.indexOf('bereich-rechtsfragen')>-1){add_position=-20;}if(klasse.indexOf('bereich-ra-money')>-1){add_position=-20;}var jf_navi_position=elem.position();var jf_bereiche=$jfjQ("#jf-bereiche");var jf_bereiche_position=jf_bereiche.position();jf_navi_position=jf_navi_position.left-jf_bereiche_position.left+10+add_position;$jfjQ(".jf-navigation").css('left',jf_navi_position);$jfjQ(".jf-navigation").css('display','block');}function checkCloseMenu(){if(window.desk_is_hovered==1){}else{$jfjQ(".jf-navigation").html(window.subnavi_std);$jfjQ(".jf-navigation").css('left',0);$jfjQ(".jf-navigation").css('display','none');}}function loadSNL(jf_bereich){if(jf_bereich.hasClass('bereich-news')){$jfjQ(".jf-navigation").html(generateSubnavi('news'));}else if(jf_bereich.hasClass('bereich-foren')){$jfjQ(".jf-navigation").html(generateSubnavi('foren'));}else if(jf_bereich.hasClass('bereich-urteile')){$jfjQ(".jf-navigation").html(generateSubnavi('urteile'));}else if(jf_bereich.hasClass('bereich-gesetze')){$jfjQ(".jf-navigation").html(generateSubnavi('gesetze'));}else if(jf_bereich.hasClass('bereich-wiki')){$jfjQ(".jf-navigation").html(generateSubnavi('wiki'));}else if(jf_bereich.hasClass('bereich-ratgeber')){$jfjQ(".jf-navigation").html(generateSubnavi('ratgeber'));}else if(jf_bereich.hasClass('bereich-rechtsanwalt')){$jfjQ(".jf-navigation").html(generateSubnavi('rechtsanwalt'));}else if(jf_bereich.hasClass('bereich-rechtsfragen')){$jfjQ(".jf-navigation").html(generateSubnavi('rechtsfragen'));}else if(jf_bereich.hasClass('bereich-ra-money')){$jfjQ(".jf-navigation").html(generateSubnavi('ra-money'));}else{$jfjQ(".jf-navigation").html("");}}function generateSubnavi(type){if(type=='news'){return'<ul><li><a title="Recht &amp; Gesetz" href="https://www.juraforum.de/recht-gesetz/">Recht &amp; Gesetz</a></li><li><a title="Arbeitsrecht" href="https://www.juraforum.de/arbeitsrecht/">Arbeitsrecht</a></li><li><a title="Verkehrsrecht" href="https://www.juraforum.de/verkehrsrecht/">Verkehrsrecht</a></li><li><a title="Mietrecht &amp; WEG" href="https://www.juraforum.de/mietrecht/">Mietrecht</a></li><li><a title="Internet &amp; IT" href="https://www.juraforum.de/internetrecht/">Internet &amp; IT</a></li><li><a title="Familie &amp; Erben" href="https://www.juraforum.de/familienrecht-erbrecht/">Familie &amp; Erben</a></li><li><a title="Wirtschaft &amp; Steuern" href="https://www.juraforum.de/wirtschaftsrecht-steuerrecht/">Wirtschaft &amp; Steuern</a></li><li><a title="Wissenschaft" href="https://www.juraforum.de/wissenschaft/">Wissenschaft</a></li><li><a title="Kanzleinews &amp; Personalia" href="https://www.juraforum.de/personalia-unternehmensnews/">Kanzleinews</a></li><li class="letzte"><a title="Rechtstipps" href="https://www.juraforum.de/ratgeber/">Rechtstipps</a></li></ul>';}else if(type=='foren'){return'<ul><li><a title="Registrierung im Forum" href="https://www.juraforum.de/forum/register">Registrierung</a></li><li><a title="Mitglieder" href="https://www.juraforum.de/forum/mitglieder/">Mitglieder</a></li><li><a title="Mitglieder" href="https://www.juraforum.de/forum/register">Neue Frage stellen</a></li><li><a title="Mitglieder" href="https://www.juraforum.de/rechtsberatung/">Frag einen Anwalt</a></li><li><a title="JuraExamen" href="https://www.juraforum.de/juraexamen/">JuraExamen</a></li></ul>';}else if(type=='urteile'){return'<ul><li><a href="https://www.juraforum.de/urteile/suche" title="Suche">Suche</a></li><li><a href="https://www.juraforum.de/u