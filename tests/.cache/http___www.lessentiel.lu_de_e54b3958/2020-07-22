<script language="javascript">
    var dfpTargetingObj = {
        
        tags: '',
        
    };
    dfpTargetingObj['path'] = window.location.pathname.split('/').slice(0,3).join('/');
    (function () {
        var path = '/';
        if (path == '/ro/' || path == '/fr/' || path == '/de/' || path == '/') {
            path = 'front';
        } else {
            path = path.replace(/^\/ro\//g,'/').replace(/^\/fr\//g,'/').replace(/^\/de\//g,'/');
        }

        TATM.adUnitFirst = true;
        TATM.init(
            'de-lessentiel.lu',
            path,
            dfpTargetingObj,
            false,
            994);
//      TATM.setMigrationFlag(true);
    })();
</script>
