        <script type="text/javascript">
            TATM.initAdUnits([{
                adUnitName: "outside-right",
                targetContainerId: "rightsky",
                sticky: false,
                callback: function (a) {
                    if (a && !a.isEmpty ) {
                        // only activate the second sky when the first one is filled
                        if (typeof injectRightsky === 'function') {
                            injectRightsky("outside-right-pos1", "rightsky-pos1"); // unitName, target
                        }
                    }
                }
            }]);
        </script>
