		<script type="text/javascript">
			var links = document.getElementsByTagName("a");
			var y = 0;
			for(i=0;i<links.length;i++){
				if(links[i] == ""){
					if(document.anchors[y].name != ""){
						document.anchors[y].style.color='#000000';
						document.anchors[y].style.textDecoration='none';
						y++;
					}
				}
		  }	
