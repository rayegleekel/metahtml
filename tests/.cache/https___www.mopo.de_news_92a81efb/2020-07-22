trackingJSON.list1 = contentID + '::' + teaserUrl;
} else {
trackingJSON.list1 += '|' + contentID + '::' + teaserUrl;
}
_satellite.track('list1View');
},
trackTeaserClick: function (contentID, contentUrl) {
s.forcedLinkTrackingTimeout = 500;
s.tl(this, 'o', contentID + '::' + contentUrl, DMN.analytics.setDTMVars(contentID + '::' + contentUrl), function () {
return false;
});
},
trackImageGallery: function (galleryID, galleryPosition, gallerySize) {
DMN.analytics.trackElementsWithDataLayerRefresh(galleryID + ' - ' + galleryPosition + '/' + gallerySize,"picGaleryElement",true);
_satellite.track('picView');
},
trackElements: function (value, paramName, singleValue) {
if (singleValue === undefined) {
singleValue = false;
}
if (singleValue) {
trackingJSON[paramName] = value;
} else {
if (!trackingJSON[paramName]) {
trackingJSON[paramName] = value;
} else {
trackingJSON[paramName] += '|' + value;
}
}
},
trackElementsWithDataLayerRefresh: function (value, paramName, singleValue) {
if (singleValue === undefined) {
singleValue = false;
}
DMN.analytics.trackElements(value, paramName,singleValue);
this.generateJSON();
},
trackEventWithDataLayerRefresh: function (paramName) {
_satellite.track(paramName);
this.generateJSON();
},
trackSocialButtons: function (sharingButton) {
DMN.analytics.trackElementsWithDataLayerRefresh(sharingButton,"sharingButton",true);
_satellite.track('share');
},
trackSocialButtonsInText: function (sharingButton) {
DMN.analytics.trackElementsWithDataLayerRefresh(sharingButton,"sharingButton",true);
_satellite.track('share_intext');
},
generateJSON: function () {
var w = document.getElementById("DMN-analytics-data");
w.innerHTML = '';
var s = document.createElement("script");
s.type = 'text/javascript';
s.text = 'dataLayer = ' + JSON.stringify(trackingJSON, null, ' ');
w.appendChild(s);
},
setDTMVars: function (list1id) {
if (typeof SetTeaserVars == 'function') {
// Wenn Function über DTM geladen wird,
// können die Variablen manuell eingestellt werden.
SetTeaserVars(list1id);
} else {
// Sonst wähle die folgenden Default Werte
//
s.linkTrackVars = 'events,eVar1,eVar2,eVar3,eVar8,eVar9,eVar10,eVar11,eVar12,eVar13,eVar14,eVar17,eVar18,eVar19,eVar33,eVar38,eVar41,eVar45,list1,prop1,prop2,prop3,prop4,prop6,prop7,prop8,prop9,prop10,prop11,prop12,prop13,prop15,prop16,prop18,prop32';
s.linkTrackEvents = 'event22';
/*Set standard vars*/
s.eVar1 = _satellite.getVar('site');
s.eVar2 = _satellite.getVar('ressort');
s.eVar3 = _satellite.getVar('category');
s.eVar8 = _satellite.getVar('adZone');
s.eVar9 = _satellite.getVar('pageType');
s.eVar10 = _satellite.getVar('pageName');
s.eVar11 = "D=g";
s.eVar12 = s.eVar13;
s.eVar14 = _satellite.getVar('channel');
s.eVar19 = _satellite.getVar('dateUnix');
s.eVar33 = _satellite.getVar('ivwZone');
/*Set standard props NOTE: set props via dynamic vars (D=) to save characters*/
s.prop1 = "D=v1";
s.prop2 = "D=v2";
s.prop3 = "D=v3";
s.prop4 = "D=v8";
s.prop6 = "D=v14";
s.prop7 = "D=v15";
s.prop8 = "D=v17";
s.prop9 = "D=v18";
s.prop10 = "D=v19";
s.prop11 = "D=v33";
s.prop12 = "D=v10";
s.prop13 = "D=v41";
s.prop15 = "D=v45";
s.prop16 = "D=v9";
s.prop32 = "D=v38";
s.events = 'event22';
s.list1 = list1id;
}
}
};
}());
}
DMN.analytics.generateJSON();
</script>
<script src="https://assets.adobedtm.com/d8109ac3d2f76151ca0f39163be0d8d6a7349f5c/satelliteLib-ac721fa85d386407eee5078eda01f3c0de309df0.js"></script>
