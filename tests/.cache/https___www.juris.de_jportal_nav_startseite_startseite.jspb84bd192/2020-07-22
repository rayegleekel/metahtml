	<script type="text/javascript">
		function initBurgerMenu(buttonSelector,menuSelector,itemSelector,exclusiveSubMenu) {
			let menuButton = document.querySelector(buttonSelector);
			let menu = document.querySelector(menuSelector);
			let navItems = document.querySelectorAll(itemSelector);
			if( navItems.length > 0 && menuButton != null && menu != null) {
				navItems.forEach((navItem) => {
					navItem.addEventListener('click',(event) => {
						event.currentTarget.classList.toggle('active');
						if( exclusiveSubMenu === true ) {
							navItems.forEach((navItem) => {
								if(navItem != event.currentTarget) {
									navItem.classList.remove('active');
								}
							});
						}
					},false);
				});
				menuButton.addEventListener('click',(event) => {
					menuButton.classList.toggle('open');
					menu.classList.toggle('collapsed');
				});
			}
		}
		initBurgerMenu('button.mobile__nav.nav__button','nav.mainnav.mobile__nav','.mobile__nav li.mainnav__item',true);	
	</script>
	<!-- END burger menu -->
