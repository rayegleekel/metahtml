				<!-- Begin DivLeftGridC1 -->
				<div class="DivLeftGridC1">
					<!-- Begin 國金要聞 -->
					<div id="International" class="DivBoxStyleC shadow GATrack" data-galabel="desktop|news|home|國金要聞" style="display:block;">
						<div class="DivTemplateB">
							<div class="DivTemplateBHdr"><a href="commentary_category.php?category=global" class="ThemeColor">國金要聞</a></div>
							<a href="commentary_category.php?category=global" class="ThemeColor"><img src="images/more.gif" alt="More" title="More" width="11" height="11" border="0" align="right" class="more" /></a>
