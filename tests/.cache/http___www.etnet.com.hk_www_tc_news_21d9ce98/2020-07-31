<script>
var TabL2 = 'Popular';
var channel_flag = 'lifestyle';
var tmp_selector = '.rightlist_' + TabL2;
if($(tmp_selector).length > 0){
	$(tmp_selector+'_NoData').hide();
}else {
	$(tmp_selector+'_NoData').show();
}

function setDiva(cname, cvalue, cday){
	setCookie(cname, cvalue, cday, false);
	window.location.reload();
}

function setColumn(cname, cvalue, cday){
	setCookie(cname, cvalue, cday, false);
	window.location.reload();
}

function change_tab(TabL2){
	TabL2 = TabL2;
	$('#CrossContainer .SliderIndicator div').click(function(){
		$(this).addClass('Selected').siblings().removeClass('Selected');
		changeSlide($(this).index());
	});
	var tmp_selector = '.CrossListContainer';
	$(tmp_selector).hide();
	$(tmp_selector + '.' + TabL2).show();
	ThumbnailHandler();
}

function changeSlide(slide_idx) {
	var max_slide_idx = $('#CrossContainer .SliderIndicator > div').length;
	if (slide_idx > max_slide_idx) slide_idx = 0;

	var left = $('#CrossPickContainer .SliderWrapper div:eq('+slide_idx+')').width() * slide_idx * -1;
	$('#CrossPickContainer .SliderWrapper').css('left', left);
}

function changeSlideLeft(){
	var max_slide_idx = $('#CrossContainer .SliderIndicator > div').length;
	var current_index = $('#CrossContainer .SliderIndicator div[class*="Selected"]').index();
	if(current_index == 0){
		next_index = max_slide_idx - 1;
	}else{
		next_index = current_index - 1;
	}
	changeSlide(next_index);
	$('#CrossContainer .SliderIndicator div:eq('+next_index+')').addClass('Selected').siblings().removeClass('Selected');
}

function changeSlideRight(){
	var max_slide_idx = $('#CrossContainer .SliderIndicator > div').length;
	var current_index = $('#CrossContainer .SliderIndicator div[class*="Selected"]').index();
	if(current_index == (max_slide_idx - 1)){
		next_index = 0;
	}else{
		next_index = current_index + 1;
	}
	changeSlide(next_index);
	$('#CrossContainer .SliderIndicator div:eq('+next_index+')').addClass('Selected').siblings().removeClass('Selected');
}

function eventActive(){
	SliderHandler();
	ThumbnailHandler();
	
	$('.CurrentTabName').html($('.SortingType ul li.Selected').data('title') + ':');
	
	$('#CrossContainer .SliderIndicator div').click(function(){
		$(this).addClass('Selected').siblings().removeClass('Selected');
		changeSlide($(this).index());
	});
	//level2
	$(".SortingType ul li").click(function(){
		if($(this).hasClass('Bookmark')){
			window.open('http://www.etnet.com.hk/www/tc/lifestyle/'+'list.php?type=bookmark');
			return;
		}
		TabL2 = $(this).data("tab");
		$(this).addClass('Selected').siblings().removeClass('Selected');
		$('.CurrentTabName').html($(this).data('title') + ':');
		change_tab(TabL2);
		//ga tracker for change tab event, "nonInteraction:1" means do not count a bounce
		ga('send', 'event', 'tab', 'click', 'desktop|news|home|RHS|三兄妹|lifestyle|Tab|Level2|'+TabL1+'|'+TabL2, {'nonInteraction': 1});
	});

	//level3
	$("div.sub_tab.SegmentContainer ul li").click(function(){
		$(this).siblings().removeClass("Selected");
		$(this).addClass("Selected");
		var current_tab = $(this).data("tab");
		var tmp_selector = '.rightlist_' + TabL2;
		if (current_tab == TabL2+'_all') {
			if($(tmp_selector).length > 0){
				$(tmp_selector+'_NoData').hide();
				$(tmp_selector).show();
			}else {
				$(tmp_selector+'_NoData').show();
			}
		} else {
			$(tmp_selector).hide();
			if($(tmp_selector + '.' + current_tab).length > 0){
				$(tmp_selector+'_NoData').hide();
				$(tmp_selector + '.' + current_tab).show();
			}else{
				$(tmp_selector+'_NoData').show();
			}
		}
		ThumbnailHandler();
		//ga tracker for change tab event, "nonInteraction:1" means do not count a bounce
		ga('send', 'event', 'tab', 'click', 'desktop|news|home|RHS|三兄妹|lifestyle|Tab|Level3|'+TabL1+'|'+TabL2+'|'+current_tab, {'nonInteraction': 1});
	});

	$('p.Comment a[data-pid]').click(function(){
		$('#findpost').val($(this).data('pid'));
		$('#FindArticleForm').attr('action', $(this).data('artlink'));
		$('#FindArticleForm').submit();
	});
	
}
</script>
			</div>
		</div>
	</div>
	<form id="FindArticleForm" target="_blank" action="" method="post">
		<input type="hidden" name="findpost" id="findpost" value="" />
	</form>
	<script>
	var TabL1 = 'Lifestyle';
	var _channel_flag = 'news';
	var _cookielist = '{"lifestyle":[],"diva":[]}';
	var _ga_label_prefix = 'desktop|news|home';
	$(document).ready(function(){
		eventActive();
		$('#CrossTab li').click(function(){
			if(!$(this).hasClass('Selected')){
				change_channel($(this).data('channel'));
			}
		});
	});

	function getCrossData(channel_tab) {
		$.ajax({
			url:'/ajax/get_cross_data.php',
			data:{'channel_tab':channel_tab, 'channel_flag':_channel_flag, 'cookielist':_cookielist, default_tab:TabL2, 'ga_label_prefix':_ga_label_prefix},
			type:'POST',
			success:function(data){
				if(data != ''){
					$('#AjaxData').html(data);
					eventActive();
				}
			}
		});
	}

	/* Formatting Slider Indicator */
	function SliderIndicatorHandler(){
		jQuery.each($("#CrossContainer .SliderIndicator"), function() {
			$(this).html('');
			var childLength=$(this).siblings(".SliderContainer").find(".SliderWrapper > div").length;
			for(var i=0;i<childLength;i++){
				$(this).append("<div></div>");
			}
			$(this).children("div:first-child").addClass("Selected");
		});
	}

	/* Formatting SliderWrapper */
	function SliderHandler(){
		jQuery.each($("#CrossContainer .SliderContainer"), function() {
			var ratioY;
			var ratioX;
			if($(this).hasClass("SLength")){
				ratioY=8.4;
				ratioX=16;
			}else if($(this).hasClass("MLength")){
				ratioY=8.4;
				ratioX=16;
			}
			$(this).find(".SliderWrapper > div").css("width",$(this).outerWidth());
			$(this).find(".Cover").outerHeight($(this).find(".SliderWrapper > div").outerWidth()*ratioY/ratioX+"px");
			$(this).find(".SliderWrapper").css("width",$(this).find(".SliderWrapper > div").length*100.05+"%");
			$(this).css("height","auto").css("height",$(this).find(".SliderWrapper").outerHeight(true));
		});
	}
	/* Set the height of .Thumbnail */
	function ThumbnailHandler(){
		jQuery.each($("#CrossContainer .Thumbnail"), function() {
			$(this).outerHeight($(this).outerWidth()*8.4/16+"px");
		});
	}
	
	function change_channel(channel)
	{
		if(!$('#CrossTab li[data-channel="'+channel+'"]').hasClass('Selected')){
			$('#CrossTab li[data-channel="'+channel+'"]').addClass("Selected").siblings().removeClass("Selected");
			getCrossData(channel.toLowerCase());
		}
	}
	</script>
</section>
