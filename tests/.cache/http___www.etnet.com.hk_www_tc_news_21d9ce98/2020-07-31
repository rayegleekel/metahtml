						<script>
							$(document).ready(function() {
								tabControl('#NewsTabControl','#NewsTabContentControl','TabActive ThemeColor',0);
								$('#NewsTabControl div').click( function() {
									if ($(this).attr('id') == 'MiddleNewsRealtime') {
										$('#NewsTabLink').attr('href', '../news/categorized_news.php');
									}else if ($(this).attr('id') == 'MiddleNewsEditorChoice') {
										$('#NewsTabLink').attr('href', '../news/categorized_news_list.php?category=editorchoice');
									}
									$('#NewsTabLink').data('galabel', 'desktop|news|home|'+$(this).html());
								});
							});
						</script>
