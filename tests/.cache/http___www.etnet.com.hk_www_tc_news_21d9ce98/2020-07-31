<script>
$(document).ready(function(){
	init();
	FocusEventActive();
});

/* Formatting Slider Indicator */
function FocusSliderIndicatorHandler(){
	jQuery.each($("#news_highlights .SliderIndicator"), function() {
		$(this).html('');
		var childLength=$(this).siblings(".SliderContainer").find(".SliderWrapper > div.SliderGroup").length;
		for(var i=0;i<childLength;i++){
			$(this).append("<div></div>");
		}
		$(this).children("div:first-child").addClass("Selected");
	});
}

/* Formatting SliderHandler */
function FocusSliderHandler(){
	jQuery.each($("#news_highlights .SliderContainer"), function() {
		var ratioY;
		var ratioX;
		if($(this).hasClass("SLength")){
			ratioY=8.4;
			ratioX=16;
		}else if($(this).hasClass("MLength")){
			ratioY=8.4;
			ratioX=16;
		}
		$(this).find(".SliderWrapper > div.SliderGroup").css("width",$(this).outerWidth());
		$(this).find(".Cover").outerHeight($(this).find(".SliderWrapper > div.SliderGroup > div").outerWidth()*ratioY/ratioX+"px");
		$(this).find(".SliderWrapper").css("width",$(this).find(".SliderWrapper > div.SliderGroup").length*100+"%");
		$(this).css("height","auto").css("height",$(this).find(".SliderWrapper").outerHeight(true));
	});
}

/* Set the height of .Thumbnail */
function FocusThumbnailHandler(){
	jQuery.each($("#news_highlights .Thumbnail"), function() {
		$(this).outerHeight($(this).outerWidth()*8.4/16+"px");
	});
}

function init(){
	posHandler();
	if($("#news_highlights .SliderIndicator").length>0){
		FocusSliderIndicatorHandler();
	}
}

function posHandler(){
	if($("#news_highlights .SliderContainer").length>0){
		FocusSliderHandler();
	}
	if($("#news_highlights .Thumbnail").length>0){
		FocusThumbnailHandler();
	}
}

function FocusEventActive(){
    FocusSliderHandler();
	FocusSliderIndicatorHandler();
	FocusThumbnailHandler();

	$('#news_highlights .SliderIndicator div').click(function(){
		$(this).addClass('Selected').siblings().removeClass('Selected');
		focuschangeSlide2($(this).index());
	});

	$('#NewsContainer .Slider.Left div').click( function(){
		focuschangeSlideLeft2();
	});

	$('#NewsContainer .Slider.Right div').click( function(){
		focuschangeSlideRight2();
	});
}

function focuschangeSlide2(slide_idx) {
	var max_slide_idx = $('#news_highlights .SliderIndicator > div').length;
	if (slide_idx > max_slide_idx) slide_idx = 0;

	var left = $('#NewsContainer .SliderWrapper div.SliderGroup:eq('+slide_idx+')').width() * slide_idx * -1;
	$('#NewsContainer .SliderWrapper').css('left', left);
}

function focuschangeSlideLeft2(){
	var max_slide_idx = $('#news_highlights .SliderIndicator > div').length;
	var current_index = $('#news_highlights .SliderIndicator div[class*="Selected"]').index();
	if(current_index == 0){
		next_index = max_slide_idx - 1;
	}else{
		next_index = current_index - 1;
	}
	focuschangeSlide2(next_index);
	$('#news_highlights .SliderIndicator div:eq('+next_index+')').addClass('Selected').siblings().removeClass('Selected');
}

function focuschangeSlideRight2(){
	var max_slide_idx = $('#news_highlights .SliderIndicator > div').length;
	var current_index = $('#news_highlights .SliderIndicator div[class*="Selected"]').index();
	if(current_index == (max_slide_idx - 1)){
		next_index = 0;
	}else{
		next_index = current_index + 1;
	}
	focuschangeSlide2(next_index);
	$('#news_highlights .SliderIndicator div:eq('+next_index+')').addClass('Selected').siblings().removeClass('Selected');
}
</script>			<!-- End Focus Highlight -->
