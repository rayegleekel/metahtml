<script>
$(document).ready(function(){
	$('.usbox .moreBtn').parent().click(function() {
		if ($('.usbox .moreBtn').hasClass('Close')) {
			$('.usContent').hide();
			$('.usbox .moreBtn').removeClass('Close');
			$('#shortRemark').show();
			$('.moreWord').html("顯示更多");
		} else {
			$('.usContent').show();
			$('.usbox .moreBtn').addClass('Close');
			$('#shortRemark').hide();
			$('.moreWord').html("隱藏");
		}
	});

	$('.usSummary li[data-adrcode]').click(function(e){
		e.stopPropagation();
		var link = '/www/tc/stocks/realtime/quote.php?code='+$(this).data('adrcode');
		ga('send', 'event', 'link', 'click', 'desktop|home|common_us_summary|quote');
		window.open(link,'etnet_stocks');
	});
});
</script>
