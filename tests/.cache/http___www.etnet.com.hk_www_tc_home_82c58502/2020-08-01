$(document).ready(function() {
	tabControl('#PopularTab','#FNewsList','Selected', 0, 'li'); // HotArticlesTabControl, HotArticlesTabs
	tabControl('#TopicNewsHotArtTabControl','#TopicNewsHotArtTabs','Selected', 0, 'li', null, 'ul');
});
</script>
