<script>
$(document).ready(function(){
	if($("#CategoryPromoContainer .SliderContainer").length>0){
		sectionPromoSliderHandler();
	}
	if($("#CategoryPromoContainer .Thumbnail").length>0){
		sectionPromoThumbnailHandler();
	}
	if($("#CategoryPromoContainer .SliderIndicator").length>0){
		SectionPromoIndicatorHandler();
	}
	CatPromoSliderActive();
});

function CatPromochangeSlide(slide_idx) {
	var max_slide_idx = $('#CategoryPromoContainer .SliderIndicator > div').length;
	if (slide_idx > max_slide_idx) slide_idx = 0;

	var left = $('#CategoryPromoArticleWrapper .SliderWrapper div:eq('+slide_idx+')').width() * slide_idx * -1;
	$('#CategoryPromoArticleWrapper .SliderWrapper').css('left', left);
}

function CatPromochangeSlideLeft(){
	var max_slide_idx = $('#CategoryPromoContainer .SliderIndicator > div').length;
	var current_index = $('#CategoryPromoContainer .SliderIndicator div[class*="Selected"]').index();
	if(current_index == 0){
		next_index = max_slide_idx - 1;
	}else{
		next_index = current_index - 1;
	}
	CatPromochangeSlide(next_index);
	$('#CategoryPromoContainer .SliderIndicator div:eq('+next_index+')').addClass('Selected').siblings().removeClass('Selected');
}

function CatPromochangeSlideRight(){
	var max_slide_idx = $('#CategoryPromoContainer .SliderIndicator > div').length;
	var current_index = $('#CategoryPromoContainer .SliderIndicator div[class*="Selected"]').index();
	if(current_index == (max_slide_idx - 1)){
		next_index = 0;
	}else{
		next_index = current_index + 1;
	}
	CatPromochangeSlide(next_index);
	$('#CategoryPromoContainer .SliderIndicator div:eq('+next_index+')').addClass('Selected').siblings().removeClass('Selected');
}

function CatPromoSliderActive(){
	sectionPromoSliderHandler();
	SectionPromoIndicatorHandler();
	sectionPromoThumbnailHandler();
	
	$('#CategoryPromoContainer .SliderIndicator div').click(function(){
		$(this).addClass('Selected').siblings().removeClass('Selected');
		CatPromochangeSlide($(this).index());
	});
	
	$('#CategoryPromoArticleWrapper .Slider.Left div').click( function(){
		CatPromochangeSlideLeft();
	});
	
	$('#CategoryPromoArticleWrapper .Slider.Right div').click( function(){
		CatPromochangeSlideRight();
	});
}
/* Formatting Slider Indicator */
function SectionPromoIndicatorHandler(){
	jQuery.each($("#CategoryPromoContainer .SliderIndicator"), function() {
		$(this).html('');
		var childLength=$(this).siblings(".SliderContainer").find(".SliderWrapper > div").length;
		for(var i=0;i<childLength;i++){
			$(this).append("<div></div>");
		}
		$(this).children("div:first-child").addClass("Selected");
	});
}

/* Formatting SliderWrapper */
function sectionPromoSliderHandler(){
	jQuery.each($("#CategoryPromoContainer .SliderContainer"), function() {
		var ratioY;
		var ratioX;
		if($(this).hasClass("SLength")){
			ratioY=8.4;
			ratioX=16;
		}else if($(this).hasClass("MLength")){
			ratioY=8.4;
			ratioX=16;
		}
		$(this).find(".SliderWrapper > div").css("width",$(this).outerWidth());
		$(this).find(".Cover").outerHeight($(this).find(".SliderWrapper > div").outerWidth()*ratioY/ratioX+"px");
		$(this).find(".SliderWrapper").css("width",$(this).find(".SliderWrapper > div").length*100.05+"%");
		$(this).css("height","auto").css("height",$(this).find(".SliderWrapper").outerHeight(true));
	});
}

/* Set the height of .Thumbnail */
function sectionPromoThumbnailHandler(){
	jQuery.each($("#CategoryPromoContainer .Thumbnail"), function() {
		$(this).outerHeight($(this).outerWidth()*8.4/16+"px");
	});
}
</script>
