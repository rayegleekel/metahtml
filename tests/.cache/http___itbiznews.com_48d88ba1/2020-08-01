<script type="text/javascript">
	var scrollR = jQuery('div[id="scroll_layer"]'); /* div의 id 속성 값을 입력 */
	var scrollR_top = 123; /* div layer top position */ 
	scrollR.css('top', 123 ); /* 초기 페이지가 로딩되었을 때 광고 top 위치 */
	jQuery (document).ready(function(){ /* 페이지 로딩이 완료되었을 때 */
		scrollR.animate( { "top":  jQuery (document).scrollTop() + scrollR_top + "px" }, 0 );
		jQuery (window).scroll(function(){
			scrollR.stop(); /* 이전의 동작을 중지 */
			/* 1초 동안 위치 이동 */ 
			scrollR.animate( { "top":  jQuery (document).scrollTop() + scrollR_top + "px" }, 1000 );
		});
	});
</script>



