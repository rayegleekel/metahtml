<script type="text/javascript">
	require(["jquery","dialog"],function( $ ,dialog){
           //  setTimeout(function(){
           $(function(){
		
                
		
		var title = "ã" + $.trim(document.title) + "ã";
                var description = $.trim($('meta[name="description"]').attr('content'));
                var portalUrl = window.location.href;
                var pic = '';
    /*qqç©ºé´åäº«*/
    function showToQzone() {
         var _desc = description;
         var _title = title;
         var _url = portalUrl;
         var _pic = pic;
         var _shareUrl = 'http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?';
         _shareUrl += 'url=' + encodeURIComponent(_url);   //åæ°urlè®¾ç½®åäº«çåå®¹é¾æ¥|é»è®¤å½åé¡µlocation
         _shareUrl += '&title=' + encodeURIComponent(_title);    //åæ°titleè®¾ç½®åäº«æ é¢ï¼å¯éåæ°
         _shareUrl += '&pics=' + _pic;
         window.open(_shareUrl,'','width=700,height=680,top=0,left=0,toolbar=no,menubar=no,scrollbars=no,resizable=1,location=yes,status=0'); 
    }
     /*å¾®ååäº«*/
    function showToSina(title, portalUrl, desc) {
        var _desc = desc;
        var _t = title + " " + _desc;
        var _url = portalUrl;
        //var _appkey = ""; //ä½ ä»å¾®èè·å¾çappkey
        var _site = ''; //ä½ çç½ç«å°å
        var _ralateUid = "";
        var _pic = pic;
        var _u = 'http://service.weibo.com/share/share.php?url=' + _url + '&title=' + _t + '&pic=' + _pic + '&ralateUid=' + _ralateUid + '&searchPic=false';
        window.open(_u, '', 'width=700, height=680, top=0, left=0, toolbar=no, menubar=no, scrollbars=no, location=yes, resizable=no, status=no');
    }

    $('.share-qqzone').on('click', function() {
        showToQzone(title, portalUrl, description);
    });
    $('.share-weibo').on('click', function() {
        showToSina(title, portalUrl, description);
    });
    $(".share-wechat").on("click", function() {
    	var Content = $("#qrcode img").attr("src");
       $(".qrconde img").attr("src",Content);
        $(".bg-code,.qrconde").toggle();
    });
    //æ¸ç©ºäºç»´ç 
    $(".close-btn").on("click", function(event) {
        $(".bg-code,.qrconde,.close-btn").hide();
        event.stopPropagation();
    });
    }, 0)

   })
