<script language="JavaScript">

    var like_yn="";


    function sharesocial(site,shareurl){
        if(site==="facebook"){
            window.open('https://www.facebook.com/sharer/sharer.php?u='+encodeURIComponent(shareurl)+'&t='+document.title, '',
                'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=300,width=600');
            return false;
        }else if(site==="twitter"){
            window.open('https://twitter.com/share?text=&url='+encodeURIComponent(shareurl) , '','menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=300,width=600');
            return false;
        }else if(site==="google"){
            window.open('https://plus.google.com/share?url='+encodeURIComponent(shareurl) , '','menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=300,width=600');
            return false;
        }
    }

    function copyurl(url){
        copyToClipboard(url);

        swal({
            type: 'success',
            title: '포스트 주소가 복사되었습니다!',
            showConfirmButton: false,
            timer: 1000
        })
    }

</script>
