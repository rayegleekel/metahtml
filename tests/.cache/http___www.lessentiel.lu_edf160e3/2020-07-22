        <script>
            $(document).ready(function() {
              var lang = $.cookie('lng_pref');

              if ( lang != "DE" && lang != "FR" )
                $('#lang_box').css('display', 'block');

              $('#lang_box .close_btn').click(function() {
                $.cookie('lng_pref', 'FR', { expires: 30, path: '/' });
                $('#lang_box').slideToggle('slow');
              });
            });
        </script>


