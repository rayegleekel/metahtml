<script type="text/javascript">
// Slider-Instanz erzeugen
var slideToFirstSlideAfterLastSlideTime = false;
wpSwipes[15519919] = $('#topMessagesSwiper').swiper({
mode:'horizontal',
loop: false,
spaceBetween: 0,
slidesPerView: 1,
centeredSlides: true,
autoplay: 7000,
speed: 1000,
autoplayStopOnLast: true,
autoplayDisableOnInteraction: false,
autoplayStopOnLast: true,
//preloadImages: false,
//lazyLoading: true,
pagination: '#topMessagesPagination-15519919',
nextButton: '#topMessagesNext-15519919',
prevButton: '#topMessagesPrev-15519919',
paginationClickable: true,
simulateTouch: wpSwipesSimulateTouch,
// lazyLoadingInPrevNext: true,
// lazyLoadingOnTransitionStart: false,
onSlideChangeStart: function( s ) {
if ( ( $( window ).width() + 16 ) >= 960 ) {
$( '#topMessagesNavigation-15519919' ).fadeToggle( 200 );
}
},
onSlideChangeEnd: function ( s ) {
if ( ( $( window ).width() + 16 ) >= 960 ) {
$( '#topMessagesNavigation-15519919' ).fadeToggle( 200 );
}
},
onReachEnd: function( s ) {
//$( '#topMessagesPlay-15519919' ).removeClass( 'swiper-button-pause' ).addClass( 'swiper-button-play');
if ( $( window ).width() + 16 >= 960 ) {
$( '#topMessagesNext-15519919' ).hide();
$( '#topMessagesNext-15519919-L' ).show();
}
slideToFirstSlideAfterLastSlideTime = setTimeout( function() {
wpSwipes[15519919].slideTo( 0 );
//$( '#topMessagesPlay-15519919' ).removeClass( 'swiper-button-pause' ).addClass( 'swiper-button-play' );
//wpSwipes[15519919].stopAutoplay();
wpSwipes[15519919].startAutoplay();
if ( $( window ).width() + 16 >= 960 ) {
$( '#topMessagesNext-15519919' ).show();
$( '#topMessagesNext-15519919-L' ).hide();
}
}, 7000 );
}
});
if ( WP_IS_PREVIEWSERVER ) {
$( '#topMessagesPlay-15519919' ).removeClass( 'swiper-button-pause' ).addClass( 'swiper-button-play' );
wpSwipes[15519919].stopAutoplay();
}
$( window ).resize( function() {
if ( $( window ).width() + 16 >= 960 ) {
if ( wpSwipes[15519919].isEnd ) {
$( '#topMessagesNext-15519919' ).hide();
$( '#topMessagesNext-15519919-L' ).show();
} else {
$( '#topMessagesNext-15519919' ).show();
$( '#topMessagesNext-15519919-L' ).hide();
}
} else {
$( '#topMessagesNext-15519919' ).hide();
$( '#topMessagesNext-15519919-L' ).hide();
}
} );
$( 'a.topMessageLink' ).focus( function() {
if ( wpSwipes[15519919].activeIndex != $(this).data( 'topmessageindex' ) ) {
wpSwipes[15519919].slideTo( $(this).data( 'topmessageindex' ) );
}
});
$( '#topMessages' ).on( 'click', '#topMessagesPagination-15519919 > span.swiper-pagination-bullet', function() {
clearTimeout( slideToFirstSlideAfterLastSlideTime );
wpSwipes[15519919].stopAutoplay();
$( '#topMessagesPlay-15519919' ).removeClass( 'swiper-button-pause' ).addClass( 'swiper-button-play' );
if ( $( window ).width() + 16 >= 960 ) {
if ( wpSwipes[15519919].isEnd ) {
$( '#topMessagesNext-15519919' ).hide();
$( '#topMessagesNext-15519919-L' ).show();
} else {
$( '#topMessagesNext-15519919' ).show();
$( '#topMessagesNext-15519919-L' ).hide();
}
}
} );
$( '#topMessagesPrev-15519919' ).click( function() {
clearTimeout( slideToFirstSlideAfterLastSlideTime );
wpSwipes[15519919].stopAutoplay();
$( '#topMessagesPlay-15519919' ).removeClass( 'swiper-button-pause' ).addClass( 'swiper-button-play' );
if ( $( window ).width() + 16 >= 960 ) {
if ( wpSwipes[15519919].isEnd ) {
$( '#topMessagesNext-15519919' ).hide();
$( '#topMessagesNext-15519919-L' ).show();
} else {
$( '#topMessagesNext-15519919' ).show();
$( '#topMessagesNext-15519919-L' ).hide();
}
}
} );
$( '#topMessagesNext-15519919' ).click( function() {
clearTimeout( slideToFirstSlideAfterLastSlideTime );
wpSwipes[15519919].stopAutoplay();
$( '#topMessagesPlay-15519919' ).removeClass( 'swiper-button-pause' ).addClass( 'swiper-button-play' );
if ( $( window ).width() + 16 >= 960 ) {
if ( wpSwipes[15519919].isEnd ) {
$( this ).hide();
$( '#topMessagesNext-15519919-L' ).show();
} else {
$( this ).show();
$( '#topMessagesNext-15519919-L' ).hide();
}
}
} );
$( '#topMessagesNext-15519919-L' ).click( function() {
clearTimeout( slideToFirstSlideAfterLastSlideTime );
wpSwipes[15519919].stopAutoplay();
$( '#topMessagesPlay-15519919' ).removeClass( 'swiper-button-pause' ).addClass( 'swiper-button-play' );
if ( $( window ).width() + 16 >= 960 ) {
if ( wpSwipes[15519919].isEnd ) {
$( this ).hide();
$( '#topMessagesNext-15519919' ).show();
} else {
$( this ).show();
$( '#topMessagesNext-15519919' ).hide();
}
}
wpSwipes[15519919].slideTo( 0 );
} );
$( '#topMessagesPlay-15519919' ).click( function() {
clearTimeout( slideToFirstSlideAfterLastSlideTime );
if ( $( this ).hasClass( 'swiper-button-play' ) ) {
$( this ).removeClass( 'swiper-button-play' ).addClass( 'swiper-button-pause' );
wpSwipes[15519919].startAutoplay();
if ( wpSwipes[15519919].isEnd ) {
wpSwipes[15519919].slideTo( 0 );
if ( $( window ).width() + 16 >= 960 ) {
$( '#topMessagesNext-15519919' ).show();
$( '#topMessagesNext-15519919-L' ).hide();
}
} else {
wpSwipes[15519919].slideNext();
}
} else if ( $( this ).hasClass( 'swiper-button-pause' ) ) {
$( this ).removeClass( 'swiper-button-pause' ).addClass( 'swiper-button-play' );
wpSwipes[15519919].stopAutoplay();
}
} );
$( '#topMessages').mouseenter( function() {
if ( $( '#topMessagesPlay-15519919' ).hasClass( 'swiper-button-pause' ) ) {
clearTimeout( slideToFirstSlideAfterLastSlideTime );
if ( wpSwipes[15519919].autoplaying ) {
wpSwipes[15519919].stopAutoplay();
}
}
} );
$( '#topMessages').mouseleave( function() {
if ( $( '#topMessagesPlay-15519919' ).hasClass( 'swiper-button-pause' ) ) {
if ( !wpSwipes[15519919].autoplaying ) {
wpSwipes[15519919].startAutoplay();;
}
}
} );
</script>
