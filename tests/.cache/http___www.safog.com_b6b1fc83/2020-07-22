//<![CDATA[
function GetTagCloud( Lang )
{
	new Ajax( '../tools/tag_cloud.php', {
		method: 'post',
		postBody: 'lang=' + Lang,
		update: $( 'TagCloud' ),
		encoding: 'utf-8',
		autoCancel: true
	}).request( );
}
function MakeAccordions( ){
	$$( '#Accordion' ).each( function( el ){
		new Accordion( el.getElements( 'dt.StretchToggle' ), el.getElements( 'dd.Stretcher' ));
	});
}
function Init( )
{
	GetTagCloud( 'de' );
	MakeAccordions( );
	InitBM( 'de', 0 );
}
window.onDomReady( Init );
//]]>
